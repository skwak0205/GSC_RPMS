	setting	'Field Type'		'attribute'
field #8
	name	'State/Region'
	businessobject	'$<attribute[attribute_StateRegion]>'
	label	'emxComponents.LocationDialog.StateRegion'
	autoheight	'FALSE'
	autowidth	'FALSE'
