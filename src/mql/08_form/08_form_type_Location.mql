	setting	'Input Type'		'textbox'
	setting	'Registered Suite'		'Components'
	setting	'Required'		'true'
	setting	'Validate'		'validateName'
	setting	'Field Type'		'basic'
field #2
	name	'Company'
	label	'emxComponents.PersonDetails.Company'
	autoheight	'FALSE'
	autowidth	'FALSE'
	edit	'FALSE'
	setting	'Registered Suite'		'Components'
	setting	'function'		'getOrgData'
	setting	'Access Function'		'isParentACompany'
	setting	'Access Program'		'emxLocation'
	setting	'program'		'emxLocation'
	setting	'Field Type'		'programHTMLOutput'
field #3
	name	'BusinessUnit'
	label	'emxComponents.LocationDialog.BusinessUnit'
	autoheight	'FALSE'
	autowidth	'FALSE'
	edit	'FALSE'
	setting	'Registered Suite'		'Components'
	setting	'function'		'getOrgData'
