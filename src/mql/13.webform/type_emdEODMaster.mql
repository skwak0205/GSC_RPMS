#del form "type_emdEODMaster";
add form "type_emdEODMaster" web
    field
        name "Name"
        label "emxDocumentCentral.Common.Name"
        businessobject  "$<name>"
        alt ""
        range ""
        update ""
        setting "Registered Suite" "LibraryCentral"
        setting "Required" "true"
        setting "Validate" "isBadNameChars"
        setting "Editable" "false"
        setting "Field Type" "basic"
    field
        name "type"
        label "emxDocumentCentral.Common.Type"
        businessobject  "$<type>"
        alt ""
        range ""
        update ""
        setting "Admin Type" "Type"
        setting "Registered Suite" "LibraryCentral"
        setting "Editable" "false"
        setting "Field Type" "basic"
    field
        name "title"
        label "emxDocumentCentral.Common.Title"
        businessobject  "$<attribute[attribute_Title].value>"
        alt ""
        range ""
        update ""
        setting "Input Type" "textfield"
        setting "Registered Suite" "LibraryCentral"
        setting "Validate" "validateForBadCharacters"
        setting "Required" "true"
        setting "Editable" "true"
        setting "Field Type" "Attribute"
    field
        name "Description"
        label "emxDocumentCentral.Common.Description"
        businessobject  "$<description>"
        alt ""
        range ""
        update ""
        setting "Input Type" "textarea"
        setting "Registered Suite" "LibraryCentral"
        setting "Required" "false"
        setting "Validate" "isBadChars"
        setting "Editable" "true"
        setting "Field Type" "basic"
    field
        name "Policy"
        label "emxDocumentCentral.Common.Policy"
        businessobject  "$<policy>"
        alt ""
        range ""
        update ""
        setting "Admin Type" "Policy"
        setting "Registered Suite" "LibraryCentral"
        setting "Editable" "false"
    field
        name "State"
        label "emxDocumentCentral.Common.State"
        businessobject  "$<current>"
        alt ""
        range ""
        update ""
        setting "Admin Type" "State"
        setting "Registered Suite" "LibraryCentral"
        setting "Required" "false"
        setting "Editable" "false"
        setting "Field Type" "basic"
    field
        name "Owner"
        label "emxDocumentCentral.Common.Owner"
        businessobject  "$<owner>"
        alt ""
        range "${COMMON_DIR}/emxFullSearch.jsp?type=PERSON_CHOOSER&showInitialResults=true&selection=single&hideHeader=true&header=emxDocumentCentral.Search.PersonSearchResults"
        update ""
        setting "Input Type" "textbox"
        setting "Admin Type" "owner"
        setting "Registered Suite" "LibraryCentral"
        setting "Required" "true"
        setting "Editable" "true"
        setting "Field Type" "basic"
    field
        name "Originator"
        label "emxDocumentCentral.Common.Originator"
        businessobject  "$<attribute[attribute_Originator].value>"
        alt ""
        range ""
        update ""
        setting "Registered Suite" "LibraryCentral"
        setting "format" "user"
        setting "Editable" "false"
        setting "Field Type" "Attribute"
    field
        name "Originated"
        label "emxDocumentCentral.Common.Originated"
        businessobject  "$<originated>"
        alt ""
        range ""
        update ""
        setting "Registered Suite" "LibraryCentral"
        setting "Required" "false"
        setting "format" "date"
        setting "Editable" "false"
        setting "Field Type" "basic"
property "installer" value "HHI"
property "application" value "LibraryCentral"
property "installed date" value "2020-05-18"
property "original name" value "type_emdEODMaster"
property "version" value "V6R2019"
;

add property form_type_emdEODMaster on program 'eServiceSchemaVariableMapping.tcl' to form 'type_emdEODMaster';