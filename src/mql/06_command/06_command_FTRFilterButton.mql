del command 'FTRFilterButton';
add command 'FTRFilterButton'
	description	'Filter button for updating the Structure Browser value'
	label	'emxProduct.Button.Filter'
	href	'${SUITE_DIR}/FeatureFilterProcess.jsp'
	alt	''
	setting	'Target Location'		'listHidden'
	setting	'Input Type'		'submit'
	setting	'Registered Suite'		'Configuration'
	setting	'Action Label'		'emxProduct.Button.Filter'
	setting	'Licensed Product'		'ENO_FTR_TP'
	setting	'Submit'		'true'
	property	'installer'	value	'MatrixOneEngineering'
	property	'application'	value	'VariantConfigurationBase'
	property	'installed date'	value	'05-08-2015'
	property	'original name'	value	'FTRFilterButton'
	property	'version'	value	'R215'
;
modify menu FTRFeatureCustomFilterToolbar add command FTRFilterButton order command FTRFilterButton 0;
#channel ==>FTRFeatureCustomFilterToolbar
add property command_FTRFilterButton on program eServiceSchemaVariableMapping.tcl to command 'FTRFilterButton';
