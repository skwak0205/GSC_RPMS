#del form 'gscPMCProjectConceptDetailsViewForm';
add form 'gscPMCProjectConceptDetailsViewForm' web
field name 'Name'
 businessobject '$<name>'
 label emxProgramCentral.Label.gscTitle
 autoheight 'FALSE'
 autowidth 'FALSE'
 edit 'FALSE'
 setting 'Registered Suite' 'ProgramCentral'
 setting 'Required' 'true'
 setting 'Group Name' 'nameCS'
 setting 'Validate' 'isBadNameChars'
 setting 'Editable' 'True'
 setting 'Field Type' 'basic'
field name 'Description'
 businessobject '$<description>'
 label 'emxProgramCentral.Common.Description'
 autoheight 'FALSE'
 autowidth 'FALSE'
 edit 'FALSE'
 setting 'Input Type' 'textarea'
 setting 'Registered Suite' 'ProgramCentral'
 setting 'Editable' 'true'
 setting 'Field Type' 'basic'
field name 'gscProjectType'
 businessobject '$<attribute[attribute_ProjectType].value>'
 label emxProgramCentral.Label.gscProjectType
 setting 'Editable' 'true'
 setting 'Field Type' 'attribute'
 setting 'Admin Type' 'attribute_ProjectType'
 setting 'Input Type' 'combobox'
 setting 'Registered Suite' 'ProgramCentral'
field name 'gscBusinessArea'
 businessobject '$<attribute[attribute_gscBusinessArea].value>'
 label emxProgramCentral.Label.gscBusinessArea
 setting 'Editable' 'true'
 setting 'Field Type' 'attribute'
 setting 'Admin Type' 'attribute_gscBusinessArea'
 setting 'Input Type' 'combobox'
 setting 'Registered Suite' 'ProgramCentral'
field name 'gscReviewType'
 businessobject '$<attribute[attribute_gscReviewType].value>'
 label emxProgramCentral.Label.gscReviewType
 setting 'Editable' 'true'
 setting 'Field Type' 'attribute'
 setting 'Admin Type' 'attribute_gscReviewType'
 setting 'Input Type' 'combobox'
 setting 'Registered Suite' 'ProgramCentral'
field name 'BusinessUnitName'
 label 'emxProgramCentral.Label.gscBusinessUnitProject'
 autoheight 'FALSE'
 autowidth 'FALSE'
 edit 'FALSE'
 setting 'Registered Suite' 'ProgramCentral'
 setting 'Required' 'false'
 setting 'function' 'selectBusUnit'
 setting 'Editable' 'true'
 setting 'program' 'emxProjectConcept'
 setting 'Field Type' 'programHTMLOutput'
field name 'EstimatedStartDate'
 businessobject '$<attribute[attribute_TaskEstimatedStartDate].value>'
 label 'emxProgramCentral.Label.gscEstimatedStartDate'
 autoheight 'FALSE'
 autowidth 'FALSE'
 edit 'FALSE'
 setting 'Registered Suite' 'ProgramCentral'
 setting 'Access Expression' '(type=="Project Concept")'
 setting 'IgnoreTimeZone' 'true'
 setting 'format' 'date'
 setting 'Editable' 'false'
 setting 'Field Type' 'attribute'
 setting 'Calendar Function' 'getCompanyNonWorkingDays'
 setting 'Calendar Program' 'emxTask'
 setting 'Update Program' 'emxProjectSpace'
 setting 'program' 'emxProjectSpace'
 setting 'Update Function' 'updateProjectDate'
field name 'owner'
 businessobject '$<owner>'
 label 'emxProgramCentral.Label.gscOwner'
 range '${COMMON_DIR}/emxFullSearch.jsp?field=TYPES=type_Person:USERROLE=Project Lead,VPLMProjectLeader&table=PMCCommonPersonSearchTable&form=PMCCommonPersonSearchForm'
 autoheight 'FALSE'
 autowidth 'FALSE'
 edit 'FALSE'
 setting 'Registered Suite' 'ProgramCentral'
 setting 'Required' 'false'
 setting 'Access Expression' 'type.Kindof[Project Concept]'
 setting 'format' 'user'
 setting 'Editable' 'true'
field name 'state'
 businessobject '$<current>'
 label 'emxProgramCentral.Label.gscState'
 autoheight 'FALSE'
 autowidth 'FALSE'
 edit 'FALSE'
 setting 'Admin Type' 'State'
 setting 'Registered Suite' 'ProgramCentral'
 setting 'Required' 'false'
 setting 'Editable' 'false'
 setting 'Field Type' 'basic'
field name 'originated'
 businessobject '$<Originated>'
 label 'emxProgramCentral.Label.gscOriginated'
 autoheight 'FALSE'
 autowidth 'FALSE'
 edit 'FALSE'
 setting 'Registered Suite' 'ProgramCentral'
 setting 'format' 'date'
 setting 'Editable' 'false'
 setting 'Field Type' 'attribute'
field name 'modified'
 businessobject '$<modified>'
 label 'emxProgramCentral.Label.gscModified'
 autoheight 'FALSE'
 autowidth 'FALSE'
 edit 'FALSE'
 setting 'Registered Suite' 'ProgramCentral'
 setting 'format' 'date'
 setting 'Editable' 'false'
 setting 'Field Type' 'attribute'
 property 'installer' value 'ENOVIAEngineering'
 property 'application' value 'ProgramManagementBase'
 property 'installed date' value '10-28-2022'
 property 'original name' value 'gscPMCProjectConceptDetailsViewForm'
 property 'version' value 'R424.HF3'
;

add property form_gscPMCProjectConceptDetailsViewForm on program eServiceSchemaVariableMapping.tcl to form 'gscPMCProjectConceptDetailsViewForm';


# Project Concept Properties - 기존 Portal Command 수정 PMCProjectConceptProperties
# form PMCProjectDetailsViewForm -> gscPMCProjectConceptDetailsViewForm / toolbar PMCProjectDetailsToolbar -> gscPMCProjectConceptDetailsToolbar
# toolbar 의 경우, gsc*Concept* 으로 새로 생성 함 90_gscCustom 참고
# 기본: mod command PMCProjectConceptProperties href '${COMMON_DIR}/emxForm.jsp?form=PMCProjectDetailsViewForm&mode=view&toolbar=PMCProjectDetailsToolbar&formHeader=emxProgramCentral.Common.FormBasics&HelpMarker=emxhelpprojectdetails&Export=false';
mod command PMCProjectConceptProperties href '${COMMON_DIR}/emxForm.jsp?form=gscPMCProjectConceptDetailsViewForm&mode=view&toolbar=gscPMCProjectConceptDetailsToolbar&formHeader=emxProgramCentral.Common.FormBasics&HelpMarker=emxhelpprojectdetails&Export=false';
