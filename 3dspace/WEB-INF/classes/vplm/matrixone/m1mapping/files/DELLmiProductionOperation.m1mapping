<?xml version="1.0" encoding="UTF-8"?>
<mapping xmlns="http://www.3ds.com/enovia-matrixone/vplm-m1/mapping" version="9">

  <!-- Package -->
  <pattern.VPLM_PACKAGE>
    <vplm.Package name="DELLmiProductionOperation" />
    <m1.package name="DELLmiProductionOperation" />

    <!-- RelationClass -->
    <pattern.VPLM_RELATIONCLASS>
      <vplm.PLMCoreInstance name="DELLmiOperationInstance" />
      <m1.relationship name="DELLmiOperationInstance" />

      <!-- Simple -->
      <pattern.VPLM_SIMPLE>
        <vplm.Simple name="V_X2DPos" />
        <m1.attribute name="DELLmiOperationInstance.V_X2DPos" local="yes" />
      </pattern.VPLM_SIMPLE>

      <!-- Simple -->
      <pattern.VPLM_SIMPLE>
        <vplm.Simple name="V_Y2DPos" />
        <m1.attribute name="DELLmiOperationInstance.V_Y2DPos" local="yes" />
      </pattern.VPLM_SIMPLE>

    </pattern.VPLM_RELATIONCLASS>

    <!-- Class -->
    <pattern.VPLM_CLASS>
      <vplm.PLMCoreReference name="DELLmiOperationReference" />
      <m1.type name="DELLmiOperationReference" />

      <!-- Simple -->
      <pattern.VPLM_SIMPLE>
        <vplm.Simple name="V_EstimatedTime" />
        <m1.attribute name="DELLmiOperationReference.V_EstimatedTime" local="yes" />
      </pattern.VPLM_SIMPLE>

      <!-- Simple -->
      <pattern.VPLM_SIMPLE>
        <vplm.Simple name="V_EstimatedTime_AddedValueRatio" />
        <m1.attribute name="DELLmiOperationReference.V_EstimatedTime_AddedValueRatio" local="yes" />
      </pattern.VPLM_SIMPLE>

      <!-- Simple -->
      <pattern.VPLM_SIMPLE>
        <vplm.Simple name="V_IsTimeProportionalToQty" />
        <m1.attribute name="DELLmiOperationReference.V_IsTimeProportionalToQty" local="yes" />
      </pattern.VPLM_SIMPLE>

      <!-- Simple -->
      <pattern.VPLM_SIMPLE>
        <vplm.Simple name="V_MeasuredTime" />
        <m1.attribute name="DELLmiOperationReference.V_MeasuredTime" local="yes" />
      </pattern.VPLM_SIMPLE>

      <!-- Simple -->
      <pattern.VPLM_SIMPLE>
        <vplm.Simple name="V_TimeMode" />
        <m1.attribute name="DELLmiOperationReference.V_TimeMode" local="yes" />
      </pattern.VPLM_SIMPLE>

      <!-- Simple -->
      <pattern.VPLM_SIMPLE>
        <vplm.Simple name="V_TrackTime" />
        <m1.attribute name="DELLmiOperationReference.V_TrackTime" local="yes" />
      </pattern.VPLM_SIMPLE>

    </pattern.VPLM_CLASS>

  </pattern.VPLM_PACKAGE>

</mapping>
