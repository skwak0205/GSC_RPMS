        <m1.attribute name="PLMResourceManikinRef.V_Weight" local="yes" />
      </pattern.VPLM_SIMPLE>

    </pattern.VPLM_EXTENSION>

    <!-- Class -->
    <pattern.VPLM_CLASS>
      <vplm.PLMCoreReference name="ErgoHuman" />
      <m1.type name="ErgoHuman" />

      <!-- Simple -->
      <pattern.VPLM_SIMPLE>
