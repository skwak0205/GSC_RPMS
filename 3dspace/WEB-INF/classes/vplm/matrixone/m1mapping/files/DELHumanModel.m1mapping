<?xml version="1.0" encoding="UTF-8"?>
<mapping xmlns="http://www.3ds.com/enovia-matrixone/vplm-m1/mapping" version="9">

  <!-- Package -->
  <pattern.VPLM_PACKAGE>
    <vplm.Package name="DELHumanModel" />
    <m1.package name="DELHumanModel" />

    <!-- Extension -->
    <pattern.VPLM_EXTENSION>
      <vplm.Extension name="DELErgoPostureExtension" />
      <m1.interface name="DELErgoPostureExtension" />

      <!-- Simple -->
      <pattern.VPLM_SIMPLE>
        <vplm.Simple name="V_ErgoPostureDofs" />
        <m1.attribute name="DELErgoPostureExtension.V_ErgoPostureDofs" local="yes" />
      </pattern.VPLM_SIMPLE>

      <!-- Simple -->
      <pattern.VPLM_SIMPLE>
        <vplm.Simple name="V_ErgoPostureFormat" />
        <m1.attribute name="DELErgoPostureExtension.V_ErgoPostureFormat" local="yes" />
      </pattern.VPLM_SIMPLE>

      <!-- Simple -->
      <pattern.VPLM_SIMPLE>
        <vplm.Simple name="V_ErgoReferential" />
        <m1.attribute name="DELErgoPostureExtension.V_ErgoReferential" local="yes" />
      </pattern.VPLM_SIMPLE>

      <!-- Simple -->
      <pattern.VPLM_SIMPLE>
        <vplm.Simple name="V_ErgoReferentialCoordinates" />
        <m1.attribute name="DELErgoPostureExtension.V_ErgoReferentialCoordinates" local="yes" />
      </pattern.VPLM_SIMPLE>

    </pattern.VPLM_EXTENSION>

    <!-- Extension -->
    <pattern.VPLM_EXTENSION>
      <vplm.Extension name="ErgoResource" />
      <m1.interface name="ErgoResource" />

      <!-- Simple -->
      <pattern.VPLM_SIMPLE>
