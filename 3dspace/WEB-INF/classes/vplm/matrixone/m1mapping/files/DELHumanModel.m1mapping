        <vplm.Simple name="V_ErgoCoordinates" />
        <m1.attribute name="ErgoResource.V_ErgoCoordinates" local="yes" />
      </pattern.VPLM_SIMPLE>

      <!-- Simple -->
      <pattern.VPLM_SIMPLE>
        <vplm.Simple name="V_ErgoFamily" />
        <m1.attribute name="ErgoResource.V_ErgoFamily" local="yes" />
      </pattern.VPLM_SIMPLE>

      <!-- Simple -->
      <pattern.VPLM_SIMPLE>
