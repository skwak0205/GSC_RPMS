    <pattern.VPLM_SIMPLE_BASIC><vplm.Simple name="owner"          				/><m1.basic name="owner"        /></pattern.VPLM_SIMPLE_BASIC>
    <pattern.VPLM_SIMPLE_BASIC><vplm.Simple name="logicalid"       				/><m1.basic name="logicalid"    /></pattern.VPLM_SIMPLE_BASIC>
    <pattern.VPLM_SIMPLE_BASIC><vplm.Simple name="physicalid"      				/><m1.basic name="physicalid"   /></pattern.VPLM_SIMPLE_BASIC>
    <pattern.VPLM_SIMPLE_BASIC><vplm.Simple name="updatestamp"                                  /><m1.basic name="updatestamp"  /></pattern.VPLM_SIMPLE_BASIC>
    <pattern.VPLM_SIMPLE_BASIC><vplm.Simple name="C_updatestamp" 				/><m1.basic    name="modified"  /><conversion  method_alias="vplminteger_m1date" /></pattern.VPLM_SIMPLE_BASIC>
    <pattern.VPLM_SIMPLE_BASIC><vplm.Simple name="V_usage"                             /><m1.basic name="to.type.name" /></pattern.VPLM_SIMPLE_BASIC>
