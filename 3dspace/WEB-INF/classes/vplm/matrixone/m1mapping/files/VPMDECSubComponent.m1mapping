<?xml version="1.0" encoding="UTF-8"?>
<mapping xmlns="http://www.3ds.com/enovia-matrixone/vplm-m1/mapping">

  <pattern.VPLM_REL>
    <vplm.PLMCoreInstance name="VPMDECInstance" read_only="yes" keep_inheritance="yes" />
    <m1.relationship name="CAD SubComponent" />

    <external key="ExpandMethod"  classname="com.matrixone.MCADIntegration.server.vplm.DECVplmExpand"            />
    <external key="PLMID"         classname="com.matrixone.vplm.identification.VPLM_PLMIDSelector"/>
    <external key="QueryServices" classname="com.matrixone.plmql.cmd.query.VPLMQuery"             />

    <pattern.VPLM_SIMPLE_CONSTANT><vplm.Simple name="cestamp"                   /><m1.constant name="00000000000000000000000000000000" /></pattern.VPLM_SIMPLE_CONSTANT>
    <pattern.VPLM_SIMPLE_CONSTANT><vplm.Simple name="V_EffectivityCompiledForm" /><m1.constant name=""                                 /></pattern.VPLM_SIMPLE_CONSTANT>
    <pattern.VPLM_SIMPLE_CONSTANT><vplm.Simple name="V_Name"                    /><m1.constant name=""                                 /></pattern.VPLM_SIMPLE_CONSTANT>
    <pattern.VPLM_SIMPLE_CONSTANT><vplm.Simple name="V_discipline"              /><m1.constant name=""                                 /></pattern.VPLM_SIMPLE_CONSTANT>
