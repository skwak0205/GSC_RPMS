    <pattern.VPLM_SIMPLE_CONSTANT><vplm.Simple name="V_hasConfigStaticMapping"  /><m1.constant name="FALSE"                            /></pattern.VPLM_SIMPLE_CONSTANT>
    <pattern.VPLM_SIMPLE_CONSTANT><vplm.Simple name="V_isUptodate"              /><m1.constant name="1"                                /></pattern.VPLM_SIMPLE_CONSTANT>
    <pattern.VPLM_SIMPLE_CONSTANT><vplm.Simple name="V_isVPLMControlled"        /><m1.constant name="FALSE"                            /></pattern.VPLM_SIMPLE_CONSTANT>
    <pattern.VPLM_SIMPLE_CONSTANT><vplm.Simple name="V_nature"             /><m1.constant name="1"                                /></pattern.VPLM_SIMPLE_CONSTANT>
    

    <pattern.VPLM_SIMPLE_CONSTANT><vplm.Simple name="V_hasInstanciationValue"   /><m1.constant name="FALSE"                            /></pattern.VPLM_SIMPLE_CONSTANT>
