    <pattern.VPLM_SIMPLE_BASIC><vplm.Simple name="majorid"         /><m1.basic name="majorid"     /></pattern.VPLM_SIMPLE_BASIC>
    
	  <pattern.VPLM_RELATIONSHIP_END><vplm.Relationship name="V_InstanceOf" /><m1.relationship end="to" /></pattern.VPLM_RELATIONSHIP_END>
    <pattern.VPLM_RELATIONSHIP_END><vplm.Relationship name="V_Owner" /><m1.relationship end="from" /></pattern.VPLM_RELATIONSHIP_END>

	</pattern.VPLM_REL>

</mapping>
