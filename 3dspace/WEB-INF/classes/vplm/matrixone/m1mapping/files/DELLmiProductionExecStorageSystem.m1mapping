<?xml version="1.0" encoding="UTF-8"?>
<mapping xmlns="http://www.3ds.com/enovia-matrixone/vplm-m1/mapping" version="9">

  <!-- Package -->
  <pattern.VPLM_PACKAGE>
    <vplm.Package name="DELLmiProductionExecStorageSystem" />
    <m1.package name="DELLmiProductionExecStorageSystem" />

    <!-- RelationClass -->
    <pattern.VPLM_RELATIONCLASS>
      <vplm.PLMCoreInstance name="DELLmiExecStorageSystemInstance" />
      <m1.relationship name="DELLmiExecStorageSystemInstance" />

    </pattern.VPLM_RELATIONCLASS>

    <!-- Class -->
    <pattern.VPLM_CLASS>
      <vplm.PLMCoreReference name="DELLmiExecStorageSystemReference" />
      <m1.type name="DELLmiExecStorageSystemReference" />

      <!-- Basic Concat -->
      <pattern.VPLM_BASIC_CONCAT>
        <vplm.Simple name="PLM_ExternalID" />
        <m1.basic name="name" />
      </pattern.VPLM_BASIC_CONCAT>

      <!-- Simple -->
      <pattern.VPLM_SIMPLE>
        <vplm.Simple name="V_ArrivalMode" />
        <m1.attribute name="DELLmiExecStorageSystemReference.V_ArrivalMode" local="yes" />
      </pattern.VPLM_SIMPLE>

      <!-- Simple -->
      <pattern.VPLM_SIMPLE>
        <vplm.Simple name="V_ArrivalTimeMode" />
        <m1.attribute name="DELLmiExecStorageSystemReference.V_ArrivalTimeMode" local="yes" />
      </pattern.VPLM_SIMPLE>

      <!-- Simple -->
      <pattern.VPLM_SIMPLE>
        <vplm.Simple name="V_InitialDelay" />
        <m1.attribute name="DELLmiExecStorageSystemReference.V_InitialDelay" local="yes" />
      </pattern.VPLM_SIMPLE>

      <!-- Simple -->
      <pattern.VPLM_SIMPLE>
        <vplm.Simple name="V_QueingMode" />
        <m1.attribute name="DELLmiExecStorageSystemReference.V_QueingMode" local="yes" />
      </pattern.VPLM_SIMPLE>

    </pattern.VPLM_CLASS>

  </pattern.VPLM_PACKAGE>

</mapping>
