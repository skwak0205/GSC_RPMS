<?xml version="1.0" encoding="UTF-8"?>
<mapping xmlns="http://www.3ds.com/enovia-matrixone/vplm-m1/mapping" version="9">

  <!-- Package -->
  <pattern.VPLM_PACKAGE>
    <vplm.Package name="DELLmiProductionOperationPPR" />
    <m1.package name="DELLmiProductionOperationPPR" />

    <!-- Extension -->
    <pattern.VPLM_EXTENSION>
      <vplm.Extension name="DELLmiOperationFilteringExt" />
      <m1.interface name="DELLmiOperationFilteringExt" />

      <!-- Simple -->
      <pattern.VPLM_SIMPLE>
        <vplm.Simple name="V_ImplicitFilteringOverload" />
        <m1.attribute name="DELLmiOperationFilteringExt.V_ImplicitFilteringOverload" local="yes" />
      </pattern.VPLM_SIMPLE>

    </pattern.VPLM_EXTENSION>

    <!-- RelationClass -->
    <pattern.VPLM_RELATIONCLASS>
      <vplm.PLMCoreInstance name="DELLmiOperationPPRInstance" />
      <m1.relationship name="DELLmiOperationPPRInstance" />

      <!-- Simple -->
      <pattern.VPLM_SIMPLE>
        <vplm.Simple name="V_Balanceable" />
        <m1.attribute name="DELLmiOperationPPRInstance.V_Balanceable" local="yes" />
      </pattern.VPLM_SIMPLE>

      <!-- Simple -->
      <pattern.VPLM_SIMPLE>
        <vplm.Simple name="V_DriftableOperation" />
        <m1.attribute name="DELLmiOperationPPRInstance.V_DriftableOperation" local="yes" />
      </pattern.VPLM_SIMPLE>

    </pattern.VPLM_RELATIONCLASS>

    <!-- Class -->
    <pattern.VPLM_CLASS>
      <vplm.PLMCoreReference name="DELLmiOperationPPRReference" />
      <m1.type name="DELLmiOperationPPRReference" />

      <!-- Simple -->
      <pattern.VPLM_SIMPLE>
        <vplm.Simple name="V_Execution_template_name" />
        <m1.attribute name="DELLmiOperationPPRReference.V_Execution_template_name" local="yes" />
      </pattern.VPLM_SIMPLE>

    </pattern.VPLM_CLASS>

  </pattern.VPLM_PACKAGE>

</mapping>
