<?xml version="1.0" encoding="UTF-8"?>
<mapping xmlns="http://www.3ds.com/enovia-matrixone/vplm-m1/mapping" version="9">

  <!-- Package -->
  <pattern.VPLM_PACKAGE>
    <vplm.Package name="DELLmiProductionSystemIOPort" />
    <m1.package name="DELLmiProductionSystemIOPort" />

    <!-- Class -->
    <pattern.VPLM_CLASS>
      <vplm.PLMPort name="DELLmiProdSystemIOPort" />
      <m1.type name="DELLmiProdSystemIOPort" mxtnr_submitted="no" />

      <!-- Simple -->
      <pattern.VPLM_SIMPLE>
        <vplm.Simple name="V_DELSystemPortPosition" />
        <m1.attribute name="DELLmiProdSystemIOPort.V_DELSystemPortPosition" local="yes" />
      </pattern.VPLM_SIMPLE>

      <!-- Simple -->
      <pattern.VPLM_SIMPLE>
        <vplm.Simple name="V_IsMainPort" />
        <m1.attribute name="DELLmiProdSystemIOPort.V_IsMainPort" local="yes" />
      </pattern.VPLM_SIMPLE>

      <!-- Simple -->
      <pattern.VPLM_SIMPLE>
        <vplm.Simple name="V_PathDirection" />
        <m1.attribute name="DELLmiProdSystemIOPort.V_PathDirection" local="yes" />
      </pattern.VPLM_SIMPLE>

      <!-- Simple -->
      <pattern.VPLM_SIMPLE>
        <vplm.Simple name="V_PathInformation" />
        <m1.attribute name="DELLmiProdSystemIOPort.V_PathInformation" local="yes" />
      </pattern.VPLM_SIMPLE>

      <!-- Simple -->
      <pattern.VPLM_SIMPLE>
        <vplm.Simple name="V_QualifiedForSourcing" />
        <m1.attribute name="DELLmiProdSystemIOPort.V_QualifiedForSourcing" local="yes" />
      </pattern.VPLM_SIMPLE>

      <!-- Simple -->
      <pattern.VPLM_SIMPLE>
        <vplm.Simple name="V_QueuingPriority" />
        <m1.attribute name="DELLmiProdSystemIOPort.V_QueuingPriority" local="yes" />
      </pattern.VPLM_SIMPLE>

    </pattern.VPLM_CLASS>

  </pattern.VPLM_PACKAGE>

</mapping>
