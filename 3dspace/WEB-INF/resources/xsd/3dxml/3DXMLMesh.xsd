<!-- edited with XMLSpy v2007 (http://www.altova.com) by Sebastien VIDECOQ (DASSAULT SYSTEMES) --><!-- @COPYRIGHT DASSAULT SYSTEMES 2006. All rights reserved. --><xs:schema xmlns="http://www.3ds.com/xsd/3DXML" xmlns:xs="http://www.w3.org/2001/XMLSchema" targetNamespace="http://www.3ds.com/xsd/3DXML" elementFormDefault="qualified" attributeFormDefault="unqualified">	<xs:include schemaLocation="../SimpleTypes/3DXMLSimpleTypes.xsd"/>	<!-- Tesselated Format 1.2 -->	<xs:complexType name="VisualizationRepType">		<xs:annotation>			<xs:documentation>A base type for tesselated XML representation. It provides a unique identifier inside the mesh to be referenced</xs:documentation>		</xs:annotation>		<xs:attribute name="id" type="xs:unsignedInt"/>	</xs:complexType>	<xs:complexType name="GraphicPrimitiveType">		<xs:annotation>			<xs:documentation>A base type for graphic primitives definition (faces, edges, Polylines, points...). It provides a unique identifier inside the mesh to be referenced.</xs:documentation>		</xs:annotation>		<xs:attribute name="id" type="xs:unsignedInt" use="optional"/>	</xs:complexType>	<xs:complexType name="FaceGPType" abstract="false">		<xs:annotation>			<xs:documentation>A graphic primitive used to represent faces of a solid or a surface. A FaceGP is made of a list of independant triangles, a list of strips, and a list of fans. Each triangle, strip and fan references vertices of the PolygonalRep  through indices in its VertexBuffer.</xs:documentation>		</xs:annotation>		<xs:complexContent>			<xs:extension base="GraphicPrimitiveType">				<xs:sequence>					<xs:element name="SurfaceAttributes" type="SurfaceAttributesType" minOccurs="0">						<xs:annotation>							<xs:documentation>Optional surface attributes that will be used to render the face. This attributes will overide attributes defined at FaceSet or PolygonalRep level.</xs:documentation>						</xs:annotation>					</xs:element>				</xs:sequence>				<xs:attribute name="triangles" type="xs:string" use="optional"/>				<xs:attribute name="strips" type="xs:string" use="optional"/>				<xs:attribute name="fans" type="xs:string" use="optional"/>			</xs:extension>		</xs:complexContent>	</xs:complexType>	<xs:complexType name="PolylineGPType">		<xs:annotation>			<xs:documentation>A graphic primitive used to represent wires or free edges of a body.</xs:documentation>		</xs:annotation>		<xs:complexContent>			<xs:extension base="GraphicPrimitiveType">				<xs:sequence>					<xs:element name="LineAttributes" type="LineAttributesType" minOccurs="0">						<xs:annotation>							<xs:documentation>Optional line attributes that will be used to render the polyline. This attributes will overide attributes defined at PolylineSet or PolygonalRep level.</xs:documentation>						</xs:annotation>					</xs:element>				</xs:sequence>				<xs:attribute name="vertices" type="xs:string"/>			</xs:extension>		</xs:complexContent>	</xs:complexType>	<xs:simpleType name="ColorFormatType" final="restriction">		<xs:restriction base="xs:string">			<xs:enumeration value="RGB"/>			<xs:enumeration value="RGBA"/>		</xs:restriction>	</xs:simpleType>	<xs:complexType name="ColorBufferType">		<xs:annotation>			<xs:documentation>type used to store colors of vertices</xs:documentation>		</xs:annotation>		<xs:simpleContent>			<xs:extension base="xs:string">				<xs:attribute name="format" type="ColorFormatType"/>			</xs:extension>		</xs:simpleContent>	</xs:complexType>	<xs:simpleType name="TextureDimensionType">		<xs:annotation>			<xs:documentation>A simple enumeration to describe if the texture bitmap has to be read as a line of pixel or a standard image (1D,2D)</xs:documentation>		</xs:annotation>		<xs:restriction base="xs:string">			<xs:enumeration value="1D"/>			<xs:enumeration value="2D"/>			<xs:enumeration value="3D"/>		</xs:restriction>	</xs:simpleType>	<xs:complexType name="TextureCoordinatesBufferType">		<xs:annotation>			<xs:documentation>type used to store texture coordinates of a facetted representation</xs:documentation>		</xs:annotation>		<xs:simpleContent>			<xs:extension base="xs:string">				<xs:attribute name="dimension" type="TextureDimensionType"/>				<xs:attribute name="channel" type="xs:unsignedInt"/>			</xs:extension>		</xs:simpleContent>	</xs:complexType>	<xs:complexType name="VertexBufferType">		<xs:annotation>			<xs:documentation>An object used to store and optimize vertices of a PolygonalRep. Each vertex is defined by a position, a normal and optional texture coordinates.		 </xs:documentation>		</xs:annotation>		<xs:sequence>			<xs:element name="Positions" type="xs:string">				<xs:annotation>					<xs:documentation>A string element containing the vertices positions. Each vertex position is defined by three floating point values, separated from the previous ones by a comma.</xs:documentation>				</xs:annotation>			</xs:element>			<xs:element name="Normals" type="xs:string" minOccurs="0">				<xs:annotation>					<xs:documentation>A string element containing the vertices normals. Each vertex normal  is defined by three floating point values, separated from the previous ones by a comma.</xs:documentation>				</xs:annotation>			</xs:element>			<xs:element name="TextureCoordinates" type="TextureCoordinatesBufferType" minOccurs="0" maxOccurs="unbounded">				<xs:annotation>					<xs:documentation>A string element containing the vertices texture coordinates. the texture coordinates of a vertex are described by one or two floating point values, separated from the previous ones by a comma.</xs:documentation>				</xs:annotation>			</xs:element>			<xs:element name="DiffuseColors" type="ColorBufferType" minOccurs="0">				<xs:annotation>					<xs:documentation>A string element containing the vertices diffuse colors components. the color of a vertex are defined  by three  or four floating point values acoording to the color format attribute. Each color definition is separated from the previous one by a comma. </xs:documentation>				</xs:annotation>			</xs:element>			<xs:element name="SpecularColors" type="ColorBufferType" minOccurs="0">				<xs:annotation>					<xs:documentation>A string element containing the vertices specular colors components. the color of a vertex are defined by three  or four floating point values, acoording to the color format attributes. Each color definition is separated from the previous one by a comma.</xs:documentation>				</xs:annotation>			</xs:element>		</xs:sequence>	</xs:complexType>	<xs:complexType name="PolygonalRepType">		<xs:annotation>			<xs:documentation>A 3D polygonal representation, used to represent bodies ( solids and open surfaces). The polygonal representation is composed of graphic primitives representating faces and edges of the mesh. These primitives are organised in sets in which graphic attributes can be shared.</xs:documentation>		</xs:annotation>		<xs:complexContent>			<xs:extension base="VisualizationRepType">				<xs:sequence minOccurs="0" maxOccurs="unbounded">					<xs:element name="SurfaceAttributes" type="SurfaceAttributesType" minOccurs="0">						<xs:annotation>							<xs:documentation>Optional surface attributes that will be used to render the faces of the polygonal representation. They will be overidden if new surface attributes are defined at the level of a set or of a face.</xs:documentation>						</xs:annotation>					</xs:element>					<xs:element name="LineAttributes" type="LineAttributesType" minOccurs="0">						<xs:annotation>							<xs:documentation>Optional line attributes that will be used to render the edges and of the polygonal representation. They will be overidden if new line attributes are defined at the level of a set or of a face.</xs:documentation>						</xs:annotation>					</xs:element>					<xs:element name="PolygonalLOD" type="PolygonalLODType" minOccurs="0" maxOccurs="unbounded">						<xs:annotation>							<xs:documentation>A collection of level of details of the polygonal representation. Each level of detail is composed of one set of faces and an accuracy in mm. The accuracy will be used to choose which level of detail is to be drawn according to the distance of the object to the camera.</xs:documentation>						</xs:annotation>					</xs:element>					<xs:element name="Faces" type="FaceSetType" minOccurs="0" maxOccurs="unbounded">						<xs:annotation>							<xs:documentation>One or several collections of FaceGP primitives representing the faces of the polygonal representation.</xs:documentation>						</xs:annotation>					</xs:element>					<xs:element name="Edges" type="PolylineSetType" minOccurs="0" maxOccurs="unbounded">						<xs:annotation>							<xs:documentation>One or several collections of PolylineGP primitives representing the edge of the polygonal representation.</xs:documentation>						</xs:annotation>					</xs:element>					<xs:element name="VertexBuffer" type="VertexBufferType" minOccurs="0">						<xs:annotation>							<xs:documentation>A collection of vertices with thier positions, normals and texture coordinates. These vertices should be referenced  by faces and lod faces of the PolygonalRep</xs:documentation>						</xs:annotation>					</xs:element>				</xs:sequence>				<xs:attribute name="accuracy" type="xs:float" use="optional"/>				<xs:attribute name="solid" type="xs:boolean" use="optional"/>			</xs:extension>		</xs:complexContent>	</xs:complexType>	<xs:complexType name="GraphicPrimitiveSetType" abstract="true">		<xs:annotation>			<xs:documentation>A collection of graphic primitives.</xs:documentation>		</xs:annotation>	</xs:complexType>	<xs:complexType name="IndexedPrimitiveSetType" abstract="true">		<xs:annotation>			<xs:documentation>A collection of graphic primitives with vertices indexed in a vertex buffer.</xs:documentation>		</xs:annotation>		<xs:complexContent>			<xs:extension base="GraphicPrimitiveSetType"/>		</xs:complexContent>	</xs:complexType>	<xs:complexType name="FaceSetType" abstract="false">		<xs:annotation>			<xs:documentation>A collection of of faces.</xs:documentation>		</xs:annotation>		<xs:complexContent>			<xs:extension base="IndexedPrimitiveSetType">				<xs:sequence>					<xs:element name="SurfaceAttributes" type="SurfaceAttributesType" minOccurs="0">						<xs:annotation>							<xs:documentation>Optional surface attributes that will be used to render the faces. Will overide attributes defined at the PolygonalRep. Can be overidden by surface attributes defined at the FaceGP level.</xs:documentation>						</xs:annotation>					</xs:element>					<xs:element name="Face" type="FaceGPType" maxOccurs="unbounded">						<xs:annotation>							<xs:documentation>One or several faces.</xs:documentation>						</xs:annotation>					</xs:element>				</xs:sequence>			</xs:extension>		</xs:complexContent>	</xs:complexType>	<xs:complexType name="PolylineSetType" abstract="false">		<xs:annotation>			<xs:documentation>A collection of polylines.</xs:documentation>		</xs:annotation>		<xs:complexContent>			<xs:extension base="GraphicPrimitiveSetType">				<xs:sequence>					<xs:element name="LineAttributes" type="LineAttributesType" minOccurs="0">						<xs:annotation>							<xs:documentation>Optional line attributes that will be used to render the polylines. Will overide line attributes defined at the PolygonalRep. Can be overidden by line attributes defined at the PolylineGP level.</xs:documentation>						</xs:annotation>					</xs:element>					<xs:element name="Polyline" type="PolylineGPType" maxOccurs="unbounded">						<xs:annotation>							<xs:documentation>One or several PolylineGP</xs:documentation>						</xs:annotation>					</xs:element>				</xs:sequence>			</xs:extension>		</xs:complexContent>	</xs:complexType>	<xs:complexType name="PolygonalLODType">		<xs:annotation>			<xs:documentation>The description of the level of detail of a poygonal rep. A level of detail is composed of one set of faces and an accuracy in mm. The accuracy will be used to choose which level of detail is to be drawn according to the distance of the object to the camera.</xs:documentation>		</xs:annotation>		<xs:sequence>			<xs:element name="Faces" type="FaceSetType">				<xs:annotation>					<xs:documentation>A collection of faces providing an approximate representation of the PolygonalRep</xs:documentation>				</xs:annotation>			</xs:element>		</xs:sequence>		<xs:attribute name="accuracy" type="xs:float"/>	</xs:complexType>	<xs:complexType name="BagRepType" abstract="false">		<xs:annotation>			<xs:documentation>A representation used to group other representations inside an XML Mesh.</xs:documentation>		</xs:annotation>		<xs:complexContent>			<xs:extension base="VisualizationRepType">				<xs:sequence>					<xs:element name="Rep" type="VisualizationRepType" maxOccurs="unbounded">						<xs:annotation>							<xs:documentation>one or several child representations.</xs:documentation>						</xs:annotation>					</xs:element>				</xs:sequence>			</xs:extension>		</xs:complexContent>	</xs:complexType>	<xs:element name="XMLRepresentation">		<xs:annotation>			<xs:documentation>The root element of a Tessellated XML Representation file</xs:documentation>		</xs:annotation>		<xs:complexType>			<xs:complexContent>				<xs:extension base="XMLRepresentationType">					<xs:attribute name="version" type="xs:string"/>				</xs:extension>			</xs:complexContent>		</xs:complexType>	</xs:element>	<xs:complexType name="XMLRepresentationType">		<xs:annotation>			<xs:documentation>type for the root element of a Tessellated XML Representation file</xs:documentation>		</xs:annotation>		<xs:sequence>			<xs:element name="Root" type="VisualizationRepType"/>		</xs:sequence>	</xs:complexType></xs:schema>