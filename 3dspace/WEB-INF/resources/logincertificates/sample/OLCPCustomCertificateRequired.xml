<?xml version="1.0" encoding="UTF-8" ?>

<!-- SCOPE:                           -->
<!--       Control level certificate. --> 

<!-- DESCRIPTION:                                                                                                           -->
<!--             OLCPCertificate Request certificate: Declaration of mandatory keys required by the requester (VPLM Server) -->

<!-- TYPE:                                                                                                                   -->
<!--             Custom certificate. Must be deployed by the customer on Server Side to control Client Supplied certificates -->

<!-- ACTIVATION:                                                                                                                                       --> 
<!--            To activate this certificate file you must copy this file in application server runtime view.                                          --> 
<!--            The target of this file is the resources directory of the matrixone warutil deployed ( example: ../webapps/ematrix/WEB-INF/resources ) -->

<certificate>
  <!-- Key exist mode-->
  <!-- only check if the name of the key is declared-->
  <!-- if the name of the key exist then the test return true-->
  <token key="DS.internal.SDV1" ruletemplate="exist" >
     <comment>New stream Descriptor ReqByServer 1</comment>
  </token>
</certificate>
