  (name, nodeType, owner?, creationInfo, modificationInfo, nodeStatus?,
   attributeList?, historyList?, businessObjectRefList?, duration?,
   nodeList?, userRefList?)>
<![INCLUDE[
  <!ELEMENT nodeStatus %ematrix-string;>
]]>
<![IGNORE[
  <!ATTLIST node nodeStatus
    (activated|completed|suspended|resumed|overridden|aborted|
     autoSuspended) #REQUIRED>
]]>
<!ELEMENT userRefList (userRef+)>
<!ATTLIST userRefList count CDATA #REQUIRED>

<!ELEMENT command
  (adminProperties, label, input, href, alt, code, userRefList?)>
<!ATTLIST command id CDATA #IMPLIED>
<!ELEMENT input %ematrix-string;>
<!ELEMENT href %ematrix-string;>
<!ELEMENT alt %ematrix-string;>

<!ELEMENT menu
  (adminProperties, label, input, href, alt,  commandRefList?, menuRefList?)>
<!ATTLIST menu id CDATA #IMPLIED>
<!ELEMENT commandRefList (commandRef+)>
<!ATTLIST commandRefList count CDATA #REQUIRED>
<!ELEMENT menuRefList (menuRef+)>
<!ATTLIST menuRefList count CDATA #REQUIRED>

<!ELEMENT channel
  (adminProperties, label, href, alt, height, commandRefList?, webreportRefList?,
   dataObjectRefList?, visibleUserList?)>
<!ATTLIST channel id CDATA #IMPLIED>
<!ELEMENT channelList (channel+)>
<!ATTLIST channelList count CDATA #REQUIRED>
<!ELEMENT webreportRefList (webreportRef+)>
<!ATTLIST webreportRefList count CDATA #REQUIRED>
<!ELEMENT dataObjectRefList (dataObjectRef+)>
<!ATTLIST dataObjectRefList count CDATA #REQUIRED>

<!ELEMENT portal
  (adminProperties, label, href, alt, channelRefList?, visibleUserList?)>
<!ATTLIST portal id CDATA #IMPLIED>
<!ELEMENT channelRefList (channelRef+)>
<!ATTLIST channelRefList count CDATA #REQUIRED>
<!ELEMENT portalList (portal+)>
<!ATTLIST portalList count CDATA #REQUIRED>

<!ELEMENT interfaceType
  (adminProperties, abstract?, derivedFromInterface?, localAttributes?, attributeDefRefList?,
   methodList?, typeRefList?, allowAllTypes?, relationshipDefRefList?,
   allowAllRelationships?, pathDefRefList?, allowAllPathTypes?, resetonclone?, resetonrevision?)>
<!ATTLIST interfaceType id CDATA #IMPLIED>
<!ELEMENT derivedFromInterface (interfaceTypeRefList+)>

<!ELEMENT expressionObject
  (adminProperties, expression?)>
<!ATTLIST expressionObject id CDATA #IMPLIED>

<!ELEMENT commandRef
  (name, order)>
<!ELEMENT order %ematrix-integer;>

<!ELEMENT menuRef
  (name, order)>

<!ELEMENT channelRef
  (name, portalRow, portalColumn, channelowner?)>
<!ELEMENT portalRow %ematrix-integer;>
<!ELEMENT portalColumn %ematrix-integer;>
<!ELEMENT channelowner %ematrix-string;>

<!ELEMENT webreportRef
  (name, order)>

<!ELEMENT dataObjectRef
  (name, order)>

<!ELEMENT dimension
  (adminProperties, unitList?)>
<!ATTLIST dimension id CDATA #IMPLIED>
<!ELEMENT unitRefList (unitRef+)>
<!ATTLIST unitRefList count CDATA #REQUIRED>
<!ELEMENT unitList (unit+)>
<!ATTLIST unitList count CDATA #REQUIRED>
<!ELEMENT unit
  (adminProperties, unitLabel, unitMultiplier, unitOffset, unitDefault?)>
<!ATTLIST unit id CDATA #IMPLIED>
<!ELEMENT unitRef %ematrix-string;>
<!ELEMENT dimensionRef %ematrix-string;>
<!ELEMENT keepformat EMPTY>
<!ELEMENT formatString %ematrix-string;>
<!ELEMENT unitLabel %ematrix-string;>
<!ELEMENT unitMultiplier %ematrix-string;>
<!ELEMENT unitOffset %ematrix-string;>
<!ELEMENT unitDefault EMPTY>

<!ELEMENT protection %ematrix-string;>
<!ATTLIST memberList count CDATA #REQUIRED>
<!ELEMENT memberList (member+)>
<!ELEMENT member (adminRef, protection) >
<!ELEMENT application
  (adminProperties, version?, memberList?)>
<!ATTLIST application id CDATA #IMPLIED>
<!ELEMENT tenantuid %ematrix-string;>
<!ELEMENT tenant
  (adminProperties, version?, memberList?, tenantuid?)>
<!ATTLIST tenant id CDATA #IMPLIED>

<!ELEMENT compositionRelId %ematrix-string;>
<!ELEMENT composeeList (physicalid+)>
<!ATTLIST composeeList count CDATA #REQUIRED>

<!ELEMENT inheritanceRule
  (adminProperties, operatesOn, appliesToList?, inheritanceRuleItemList?)>
<!ATTLIST inheritanceRule id CDATA #IMPLIED>
<!ELEMENT operatesOn %ematrix-string;>
<!ELEMENT appliesToList (relationshipDefRef+)>
<!ATTLIST appliesToList count CDATA #REQUIRED>
<!ELEMENT inheritanceRuleItemList (inheritanceRuleItem+)>
<!ATTLIST inheritanceRuleItemList count CDATA #REQUIRED>
<!ELEMENT inheritanceRuleItem
  (name, inheritanceRuleKind, fromEnd?, toEnd?, access?)>
<!ELEMENT inheritanceRuleKind %ematrix-string;>
<!ELEMENT fromEnd
  (allowAllTypes?, typeRefList?, allowAllPolicies?, policyRefList?)>
<!ELEMENT toEnd
  (allowAllTypes?, typeRefList?, allowAllPolicies?, policyRefList?)>
<!ELEMENT allowAllPolicies EMPTY>
<!ELEMENT policyRefList (policyRef+)>
<!ATTLIST policyRefList count CDATA #REQUIRED>

<!ELEMENT stateRule
  (name?, kind?, policyRef?, stateDefRef?, ruleUser?, ruleKey?, stateUser?, stateKey?)>
<!ELEMENT ruleUser %ematrix-string;>
<!ELEMENT ruleKey %ematrix-string;>
<!ELEMENT stateUser %ematrix-string;>
<!ELEMENT stateKey %ematrix-string;>
