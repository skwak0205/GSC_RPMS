  <Template syp:name="CAT3DComposeWkbAccess" Target="CATCmdWorkbench">
    <CATCmdWorkbench Name="CAT3DComposeWkb">

      <!-- Fixed area -->
      <CATCmdContainer Name="PADFixedArea" RepresentationType="AfrActionBarFixedSection">
        <CATCmdStarter Name="CATD3CNavigationStartStr" Command="CATC3DNavigationStartHdr" />
        <CATCmdStarter Name="CAT3DComposeExamineSelectionStr" Command="CAT3DComposeExamineSelectionHdr"/>
        <CATCmdStarter Name="CATC3DHomeStr" Command="Home"/>
      </CATCmdContainer>

      <!-- Edit toolbar -->
      <CATCmdContainer Name="CATC3DEditSct" RepresentationType="AfrActionBarSection">
        <CATCmdStarter Name="CAT3DComposeMoveStr" Command="CAT3DComposeMoveHdr"/>
        <CATCmdStarter Name="CATC3DInsertExistingProductStr" Command="UPSAuthInsertExistingProductOrPart"/>
        <CATCmdStarter Name="CATC3DReplaceByExistingStr" Command="UPSAuthReplaceByExisting"/>
        <CATCmdStarter Name="CATC3DDetachProductStr" Command="UPSAuthUnparent"/>
        <CATCmdStarter Name="CATC3DAssemblyPatternStr" Command="CAT3DComposeAssemblyPatternHdr"/>
      </CATCmdContainer>

      <!-- View toolbar -->
      <CATCmdContainer Name="PADView" RepresentationType="AfrActionBarSection">
        <CATCmdStarter  Name="CATC3DLeftSidePanelStr" Command="RootPanel"/>
        <CATCmdSeparator Name="CATC3DViewSep1"/>

        <CATCmdStarter Command="VisuReframeView" Name="PADPADReframeCmd"/>
        <CATCmdStarter Command="VisuPanView" Name="PADPanCmd"/>
        <CATCmdStarter Command="VisuRotateView" Name="PADRotateCmd"/>
        <CATCmdStarter Command="VisuZoomView" Name="PADZoomCmd"/>

        <!-- Views -->
        <CATCmdContainer Name="PADCameraViews" RepresentationType="AfrActionBarDropDown">
          <CATCmdStarter Command="VisuIsoView" Name="PADIsoViewCmd"/>
          <CATCmdStarter Command="VisuBottomView" Name="PADBottomViewCmd"/>
          <CATCmdStarter Command="VisuTopView" Name="PADTopViewCmd"/>
          <CATCmdStarter Command="VisuRightView" Name="PADRightViewCmd"/>
          <CATCmdStarter Command="VisuLeftView" Name="PADLeftViewCmd"/>
          <CATCmdStarter Command="VisuBackView" Name="PADBackViewCmd"/>
          <CATCmdStarter Command="VisuFrontView" Name="PADFrontViewCmd"/>
        </CATCmdContainer>

        <CATCmdStarter Command="VisuVisibleSpace" Name="VisuVisibleSpaceCmd"/>
        <CATCmdStarter Name="VisuViewPanelCommand" Command="VisuViewPanelCommand" />
        <CATCmdSeparator Name="CATC3DViewSep2"/>
        <CATCmdStarter Name="FlyWalkStr" Command="FlyWalkCmd"/>
        <CATCmdSeparator Name="CATC3DViewSep3"/>
        <CATCmdStarter Command="CleanAll" Name="CATC3DCleanAllStr"/>
        <CATCmdSeparator Name="CATC3DViewSep4" />
        <CATCmdStarter Name="CATC3DRightSidePanelStr" Command="RightSidePanel" />

      </CATCmdContainer>

      <!-- Tools toolbar -->
      <CATCmdContainer Name="PADTools" RepresentationType="AfrActionBarSection">
        <CATCmdStarter Command="VolumeQueryHdr" Name="3DSelection"/>
        <CATCmdStarter Command="ActionBar_Attributes" Name="ActionBar_AttributesCmd"/>
        <CATCmdStarter Command="PAD3DViewerRelationalNavigatorHdr" Name="PAD3DViewerRelationalNavigatorCmd"/>
        <CATCmdStarter Name="CATC3DDefineFilterStr" Command="DefineFilterHdr"/>
        <CATCmdStarter Name="SwitchAuthoringIndex" Command="SwitchAuthoringIndex"/>
        <CATCmdSeparator Name="CATC3DToolsSep2"/>
        <CATCmdStarter Name="CAT3DComposeSectionStr" Command="section2CmdHdr" />
        <CATCmdStarter Name="CAT3DComposeLockUnlockSectionStr" Command="LockUnlockSectionHdr" />
        <CATCmdStarter Name="CAT3DComposeMeasureStr" Command="measure3CmdHdr" />
        <CATCmdStarter Name="CAT3DComposeRevealRelatedDataStr" Command="CATRevealRelatedDataHdr"/>
        <CATCmdStarter Name="CATRelatedData3DGridStr" Command="CATRelatedData3DGridHdr"/>
        <CATCmdStarter Name="PAD3DViewerPrintCmd" Command="PAD3DViewerPrintHdr"/>
        <CATCmdSeparator Name="CATC3DToolsSep1"/>
        <CATCmdStarter Name="CATWebUXPreferencesStr" Command="CATWebUXPreferencesHdr" />
      </CATCmdContainer>
    </CATCmdWorkbench>
  </Template>

