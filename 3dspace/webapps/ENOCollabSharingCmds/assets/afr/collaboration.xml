                         <CATCmdStarter Name="Transfer Ownership" Command="ActionBar_TransferOwnershipCmd" /> 
                         <CATCmdStarter Name="Reserve" Command="ActionBar_ReserveCmd" />
                         <CATCmdStarter Name="Unreserve" Command="ActionBar_UnreserveCmd" />
                                                  
                </CATCmdContainer>
            </CATCmdWorkbench>
      </Template>
</Styles>
