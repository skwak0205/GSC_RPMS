		          <CATCommandHeader ID="DeleteLibrary" ClassName="DS/ClassificationEditor/commands/ClassificationIconBar_DeleteLibrary" ResourceFile="ClassificationEditor/ENOLibrary" Available="1"/>
				  <CATCommandHeader ID="CreateNewClass" ClassName="DS/ClassificationEditor/commands/ClassificationIconBar_CreateClass" ResourceFile="ClassificationEditor/ENOLibrary" Available="1"/>
				  <CATCommandHeader ID="DeleteClass" ClassName="DS/ClassificationEditor/commands/ClassificationIconBar_DeleteClass" ResourceFile="ClassificationEditor/ENOLibrary" Available="1"/>			 
				  <CATCommandHeader ID="MoveClass" ClassName="DS/ClassificationEditor/commands/ClassificationIconBar_MoveClass" ResourceFile="ClassificationEditor/ENOLibrary" Available="1"/>
				  <CATCommandHeader ID="MaturityHdr" ClassName="DS/LifecycleCmd/MaturityCmd" ResourceFile="LifecycleWidget/LifecycleCmd" Available="1" />
			      <CATCommandHeader ID="ActionBar_AccessRightCmd" ClassName="DS/ENOCollabSharingCmds/commands/AccessRightCmd/ActionBar_AccessRightCmd" ResourceFile="ENOCollabSharingCmds/langSharingCmds" Available="1" /> 
			</CATCmdHeadersList>
      </Template>

      <Template syp:name="ENOLibrariesWkbAccess" Target="CATCmdWorkbench">
            <CATCmdWorkbench Name="ENOLibrariesWkb">
                  <CATCmdContainer Name="Library" RepresentationType="AfrActionBarSection">
                        <CATCmdStarter Name="Add to Favorites" Command="AddToFavorites" />
                        <CATCmdStarter Name="Attributes" Command="ActionBar_Attributes" />
                  </CATCmdContainer>
          		  <CATCmdContainer Name="View" RepresentationType="AfrActionBarSection">
						<CATCmdContainer Name="SwitchView" RepresentationType="AfrActionBarDropDown">
							<CATCmdStarter Name="Grid View" Command="treeview" />
							<CATCmdStarter Name="Tile View" Command="tileview" />
							<CATCmdStarter Name="Thumbnail View" Command="thumbnailview" />
						</CATCmdContainer>
						<CATCmdContainer Name="PADExpand" RepresentationType="AfrActionBarDropDown">
							<CATCmdStarter Name="Expand All" Command="ExpandAll" />
							<CATCmdStarter Name="Expand N Levels" Command="ExpandN" />
							<CATCmdStarter Name="Collapse All" Command="CollapseAll" />
						</CATCmdContainer>
						<CATCmdContainer Name="PADSelect" RepresentationType="AfrActionBarDropDown">
							<CATCmdStarter Name="Select All" Command="SelectAll" />
							<CATCmdStarter Name="Select Inversion" Command="SelectInversion" />
							<CATCmdStarter Name="Select Siblings" Command="SelectSiblings" />
						</CATCmdContainer>
                  </CATCmdContainer>
                   <CATCmdContainer Name="AttributesMgm" RepresentationType="AfrActionBarSection">
                        <CATCmdStarter Name="Export Attributes" Command="ExportAttributes" />
                  </CATCmdContainer>
            </CATCmdWorkbench>
      </Template>
</Styles>
