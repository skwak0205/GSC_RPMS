<?xml version="1.0" encoding="utf-8" ?>

<Styles xmlns:syp="http://www.3ds.com/xsd/SYP">

  <Template syp:name="3DPlayDMUExperienceHeaders" Target="CATCmdHeadersList">
    <CATCmdHeadersList>
      <CATCommandHeader ID="PIMInteferenceListHdr" ClassName="DS/PIMwebViewController/PIMwebInteferenceListCmd" HelpResourceFile="PIMwebViewController/PIMwebViewController" ResourceFile="PIMwebViewController/PIMwebViewController" Behavior="CATAfrHeaderCheck" />
      <CATCommandHeader ID="PIMDisplayClashCurveHdr" ClassName="DS/PIMwebViewController/PIMwebDisplayCmd" HelpResourceFile="PIMwebViewController/PIMwebViewController" ResourceFile="PIMwebViewController/PIMwebViewController" Behavior="CATAfrHeaderCheck" />
      <CATCommandHeader ID="PIMDisplayContactCurveHdr" ClassName="DS/PIMwebViewController/PIMwebDisplayCmd" HelpResourceFile="PIMwebViewController/PIMwebViewController" ResourceFile="PIMwebViewController/PIMwebViewController" Behavior="CATAfrHeaderCheck" />
      <CATCommandHeader ID="PIMDisplayClearanceCurveHdr" ClassName="DS/PIMwebViewController/PIMwebDisplayCmd" HelpResourceFile="PIMwebViewController/PIMwebViewController" ResourceFile="PIMwebViewController/PIMwebViewController" Behavior="CATAfrHeaderCheck" />
      <CATCommandHeader ID="PIMDisplayQuantifiersHdr" ClassName="DS/PIMwebViewController/PIMwebDisplayCmd" HelpResourceFile="PIMwebViewController/PIMwebViewController" ResourceFile="PIMwebViewController/PIMwebViewController" Behavior="CATAfrHeaderCheck" />
      <CATCommandHeader ID="PIMZoomOnInterferenceHdr" ClassName="DS/PIMwebViewController/PIMwebDisplayCmd" HelpResourceFile="PIMwebViewController/PIMwebViewController" ResourceFile="PIMwebViewController/PIMwebViewController" Behavior="CATAfrHeaderCheck" />
      <CATCommandHeader ID="PIMApplyTransparencyHdr" ClassName="DS/PIMwebViewController/PIMwebDisplayCmd" HelpResourceFile="PIMwebViewController/PIMwebViewController" ResourceFile="PIMwebViewController/PIMwebViewController" Behavior="CATAfrHeaderCheck" />
      <CATCommandHeader ID="PIMColorOverloadHdr" ClassName="DS/PIMwebViewController/PIMwebDisplayCmd" HelpResourceFile="PIMwebViewController/PIMwebViewController" ResourceFile="PIMwebViewController/PIMwebViewController" Behavior="CATAfrHeaderCheck" />
      <CATCommandHeader ID="CATWebUXPreferencesHdr" ClassName="DS/CATWebUXPreferences/CATWebUXPreferencesCmd" HelpResourceFile = "CATWebUXPreferences/CATWebUXPreferences" ResourceFile="CATWebUXPreferences/CATWebUXPreferences"/>
      <CATCommandHeader ID="PIMwebExportFilteredHdr" ClassName="DS/PIMwebViewController/PIMwebExportAsCSVCmd" HelpResourceFile="PIMwebViewController/PIMwebViewController" ResourceFile="PIMwebViewController/PIMwebViewController" >
        <CATAfrCommandHeaderArgs>
          <CATCommandHeaderArg type="Filtered" />
        </CATAfrCommandHeaderArgs>
      </CATCommandHeader>

      <CATCommandHeader ID="PIMwebExportSelectedHdr" ClassName="DS/PIMwebViewController/PIMwebExportAsCSVCmd" HelpResourceFile="PIMwebViewController/PIMwebViewController" ResourceFile="PIMwebViewController/PIMwebViewController" >
        <CATAfrCommandHeaderArgs>
          <CATCommandHeaderArg type="Selected" />
        </CATAfrCommandHeaderArgs>
      </CATCommandHeader>
      
      <CATCommandHeader ID="CATWebUXSlideShowHdr"  ClassName="DS/CATWebUXSlideShow/CATWebUXSlideShowCmd" HelpResourceFile="CATWebUXSlideShow/CATWebUXSlideShow" ResourceFile="CATWebUXSlideShow/CATWebUXSlideShow" Behavior="CATAfrHeaderCheck">
        <CATAfrCommandHeaderArgs>
          <CATCommandHeaderArg ID="ControllerID" Value="PIMwebSlideShow" Mode="In" />
        </CATAfrCommandHeaderArgs>
      </CATCommandHeader>

      <CATCommandHeader ID ="measure2CmdHdr" ClassName="DS/3DPlayCommands/Proxy/CmdMeasureProxy" HelpResourceFile="DMUMeasure/DMUMeasure" ResourceFile="DMUBaseCommands/3DPlayPro">
        <CATAfrCommandHeaderArgs>
          <CATCommandHeaderArg ID = "3dPlay" Value = "3dPlay" Mode = "In" />
        </CATAfrCommandHeaderArgs>
      </CATCommandHeader>
      <CATCommandHeader ID="section2CmdHdr" ClassName="DS/3DPlayCommands/Proxy/CmdSectionProxy" HelpResourceFile="DMUSection/DMUSection" ResourceFile="DMUBaseCommands/3DPlayPro">
        <CATAfrCommandHeaderArgs>
          <CATCommandHeaderArg ID = "3dAuthoring" Value = "3dAuthoring" Mode = "In" />
        </CATAfrCommandHeaderArgs>
      </CATCommandHeader>
      <CATCommandHeader ID="LockUnlockSectionHdr" ClassName="DS/DMUSection/DMULockUnlockSection" HelpResourceFile="DMUSection/DMUSection" ResourceFile="DMUBaseCommands/3DPlayPro" Behavior="CATAfrHeaderCheck" />

      <CATCommandHeader ID="ShareTo3DSwYm" Available="0" ClassName="DS/3DPlayCommands/CmdShareTo3DSwYm" HelpResourceFile = "3DPlayCommands/3DPlayHelpForCommands" ResourceFile="3DPlay/3DPlay"  />
      <CATCommandHeader ID="SharePrint" Available="0" ClassName="DS/3DPlayCommands/CmdSharePrint" HelpResourceFile = "3DPlayCommands/3DPlayHelpForCommands" ResourceFile="3DPlay/3DPlay"  />
      <CATCommandHeader ID="ShareDownload" Available="0" ClassName="DS/3DPlayCommands/CmdShareDownload" HelpResourceFile = "3DPlayCommands/3DPlayHelpForCommands" ResourceFile="3DPlay/3DPlay"  />

      <CATCommandHeader ID="RightSidePanel" ClassName="DS/PIMwebViewController/PIMwebRightSidePanelCmd" HelpResourceFile="PADUtils/PADUtilsHelpForCommands" ResourceFile="PADUtils/PADUtils" Behavior="CATAfrHeaderCheck">
        <CATAfrCommandHeaderArgs>
          <CATCommandHeaderArg ID="PropertiesReadOnly" Value="true" Mode="In" />
        </CATAfrCommandHeaderArgs>
      </CATCommandHeader>

      <CATCommandHeader ID = "VisuZoomView"                   ClassName = "DS/ViewerCommands/VisuZoomViewCmd"                   ResourceFile = "ViewerCommands/ViewerCommands"  HelpResourceFile = "ViewerCommands/ViewerCommandsIDs" SharedLibraryName = "ShareUI"  />
      <CATCommandHeader ID = "VisuPanView"                    ClassName = "DS/ViewerCommands/VisuPanViewCmd"                    ResourceFile = "ViewerCommands/ViewerCommands"  HelpResourceFile = "ViewerCommands/ViewerCommandsIDs" SharedLibraryName = "ShareUI"  />
      <CATCommandHeader ID = "VisuRotateView"                 ClassName = "DS/ViewerCommands/VisuRotateViewCmd"                 ResourceFile = "ViewerCommands/ViewerCommands"  HelpResourceFile = "ViewerCommands/ViewerCommandsIDs" SharedLibraryName = "ShareUI"  />
      <CATCommandHeader ID = "VisuReframeView"                ClassName = "DS/ViewerCommands/VisuReframeViewCmd"                ResourceFile = "ViewerCommands/ViewerCommands"  HelpResourceFile = "ViewerCommands/ViewerCommandsIDs" SharedLibraryName = "ShareUI"  />
      <CATCommandHeader ID="VisuViewPanelCommand" ClassName="DS/ViewerCommands/VisuViewPanelCommand" ResourceFile="ViewerCommands/ViewerCommands" HelpResourceFile="ViewerCommands/ViewerCommandsIDs" />

      <CATCommandHeader ID = "VisuNoEnv"          ClassName = "DS/ViewerCommands/VisuNoEnvCmd"                                  ResourceFile = "ViewerCommands/ViewerCommands"  HelpResourceFile = "ViewerCommands/ViewerCommandsIDs" SharedLibraryName = "ShareUI" />
      <CATCommandHeader ID = "VisuV6Env"          ClassName = "DS/ViewerCommands/VisuV6EnvCmd"                                  ResourceFile = "ViewerCommands/ViewerCommands"  HelpResourceFile = "ViewerCommands/ViewerCommandsIDs" SharedLibraryName = "ShareUI"  />
      <CATCommandHeader ID = "VisuCleanSpaceEnv"  ClassName = "DS/ViewerCommands/VisuCleanSpaceEnvCmd"                          ResourceFile = "ViewerCommands/ViewerCommands"  HelpResourceFile = "ViewerCommands/ViewerCommandsIDs" SharedLibraryName = "ShareUI"  />
      <CATCommandHeader ID = "VisuDarkBlueEnv"    ClassName = "DS/ViewerCommands/VisuDarkBlueEnvCmd"                            ResourceFile = "ViewerCommands/ViewerCommands"  HelpResourceFile = "ViewerCommands/ViewerCommandsIDs" SharedLibraryName = "ShareUI"  />
      <CATCommandHeader ID = "VisuDarkGreyEnv"    ClassName = "DS/ViewerCommands/VisuDarkGreyEnvCmd"                            ResourceFile = "ViewerCommands/ViewerCommands"  HelpResourceFile = "ViewerCommands/ViewerCommandsIDs" SharedLibraryName = "ShareUI"  />
      <CATCommandHeader ID = "VisuShinyEnv"       ClassName = "DS/ViewerCommands/VisuShinyEnvCmd"                               ResourceFile = "ViewerCommands/ViewerCommands"  HelpResourceFile = "ViewerCommands/ViewerCommandsIDs" SharedLibraryName = "ShareUI"  />

      <CATCommandHeader ID="FlyWalkCmd" ClassName="DS/FlyWalkCommand/FlyWalkCmd" HelpResourceFile="FlyWalkCommand/FlyWalkCommandIDs" ResourceFile="FlyWalkCommand/FlyWalkCmd" Behavior="CATAfrHeaderCheck"/>
      <CATCommandHeader ID = "ViewSelector"       ClassName = "DS/3DPlayCommands/CmdViewSelector"                                   ResourceFile = "3DPlay/3DPlayExperience3D"                     HelpResourceFile = "3DPlayCommands/3DPlayHelpForCommands3D" />
      <CATCommandHeader ID = "VisibilityCommands" ClassName = "DS/3DPlayCommands/VisibilityCommands" HelpResourceFile = "3DPlayCommands/3DPlayHelpForCommands3D" ResourceFile = "3DPlay/3DPlayExperience3D" />
      <CATCommandHeader ID = "AnnotationCommands" ClassName = "DS/3DPlayAnnotation3D/AnnotationCommands" HelpResourceFile = "3DPlayAnnotation3D/3DPlayHelpForCommands3D" ResourceFile = "3DPlay/3DPlayExperience3D" />
      <CATCommandHeader ID = "AnnotationCommand3DEditShape" ClassName="DS/3DPlayAnnotation3D/AnnotationCommandEditShape" HelpResourceFile = "3DPlayAnnotation3D/3DPlayHelpForCommands3D" ResourceFile="3DPlay/3DPlayExperience3D" />
      <CATCommandHeader ID = "AnnotationCommand3DText" ClassName="DS/3DPlayAnnotation3D/AnnotationCommandText" HelpResourceFile = "3DPlayAnnotation3D/3DPlayHelpForCommands3D" ResourceFile="3DPlay/3DPlayExperience3D" />
      <CATCommandHeader ID = "AnnotationTour" ClassName = "DS/3DPlayAnnotation3D/AnnotationTour"                     HelpResourceFile = "3DPlayCommands/3DPlayHelpForCommands3D"           ResourceFile = "3DPlay/3DPlayExperience3D" />

    </CATCmdHeadersList>
  </Template>

  <Template syp:name="3DPlayDMUExperienceAccess" Target="CATCmdWorkbench">
    <CATCmdWorkbench Name="3DPlayDMUExperienceWorkbench">

      <CATCmdContainer Name="FixedArea" RepresentationType="AfrActionBarFixedSection">
        <CATCmdStarter Name="PIMwebItfShowStr" Command="CATWebUXSlideShowHdr" />
        <CATCmdSeparator Name = "FixedAreaSep"/>
        <CATCmdContainer Name = "Share" Visibility = "1" RepresentationType="AfrActionBarDropDown" >
          <CATCmdStarter Name = "ShareTo3DSwYm"  Command = "ShareTo3DSwYm" />
          <CATCmdStarter Name = "ShareDownload"  Command = "ShareDownload" />
          <CATCmdStarter Name = "SharePrint"  Command = "SharePrint" />
        </CATCmdContainer>
      </CATCmdContainer>

      <CATCmdContainer Name="PIMwebItfSct" RepresentationType="AfrActionBarSection">
        <CATCmdStarter Name="PIMInteferenceListStr" Command="PIMInteferenceListHdr"/>
        <CATCmdSeparator Name="PIMwebItfSep1"/>
        <CATCmdContainer Name="PIMwebSectionIcb" RepresentationType="AfrActionBarDropDown">
          <CATCmdStarter Name="PIMwebSectionStr" Command="section2CmdHdr" />
          <CATCmdStarter Name="PIMwebLockUnlockSectionStr" Command="LockUnlockSectionHdr" />
        </CATCmdContainer>
        <CATCmdStarter Name="PIMwebMeasureStr" Command="measure2CmdHdr" />
        <CATCmdSeparator Name="PIMwebItfSep2"/>
        <CATCmdStarter Name="PIMwebDisplayClashCurveStr" Command="PIMDisplayClashCurveHdr" />
        <CATCmdStarter Name="PIMwebDisplayContactCurveStr" Command="PIMDisplayContactCurveHdr" />
        <CATCmdStarter Name="PIMwebDisplayClearanceCurveStr" Command="PIMDisplayClearanceCurveHdr" />
        <CATCmdStarter Name="PIMwebDisplayQuantifiersStr" Command="PIMDisplayQuantifiersHdr" />
        <CATCmdStarter Name="PIMwebZoomOnInterferenceStr" Command="PIMZoomOnInterferenceHdr" />
        <CATCmdStarter Name="PIMwebColorOverloadStr" Command="PIMColorOverloadHdr" />
        <CATCmdStarter Name="PIMwebItfApplyTransparencyStr" Command="PIMApplyTransparencyHdr" />
      </CATCmdContainer>

      <CATCmdContainer Name = "PIMwebViewSct" Visibility = "1" RepresentationType="AfrActionBarSection">
        <CATCmdStarter Name = "VisuReframeView"       Command = "VisuReframeView" />
        <CATCmdStarter Name = "VisuPanView"           Command = "VisuPanView"     />
        <CATCmdStarter Name = "VisuRotateView"        Command = "VisuRotateView"  />
        <CATCmdStarter Name = "VisuZoomView"          Command = "VisuZoomView"    />
        <CATCmdStarter Name = "ViewSelector"  Command = "ViewSelector" />
        <CATCmdStarter Name="VisuViewPanelCommand" Command="VisuViewPanelCommand" />
        <CATCmdSeparator Name="PIMwebViewSep1"/>
        <CATCmdStarter Name="FlyWalkStr" Command="FlyWalkCmd"/>
        <CATCmdSeparator Name="PIMwebViewSep2" />
        <CATCmdStarter Name="PIMwebRightSidePanelStr" Command="RightSidePanel" />
      </CATCmdContainer>

      <CATCmdContainer Name = "PIMWebToolsSct" Visibility = "1" RepresentationType="AfrActionBarSection">
        <CATCmdStarter Name = "VisibilityCommands"  Command = "VisibilityCommands" />
        <CATCmdStarter Name = "AnnotationCommands"  Command = "AnnotationCommands"  />
        <CATCmdStarter Name = "AnnotationCommand3DText"  Command = "AnnotationCommand3DText"  />
        <CATCmdStarter Name = "AnnotationTour"  Command = "AnnotationTour"  />
        <CATCmdSeparator Name="PIMwebToolsSep" />
        <CATCmdStarter Name="CATWebUXPreferencesStr" Command="CATWebUXPreferencesHdr" />
      </CATCmdContainer>

    </CATCmdWorkbench>
  </Template>
</Styles>
