                              <CATCmdStarter Name="Print" Command="Print" />
                              <CATCmdStarter Name="Print Options" Command="PrintOptions" />
                        </CATCmdContainer> -->
                        <CATCmdContainer Name="ExportDropDown" RepresentationType="AfrActionBarDropDown">
                              <CATCmdStarter Name="Export CSV" Command="ExportCSV" />
                              <CATCmdStarter Name="UnifiedExportCommand" Command="UnifiedExportCommand" />
                        </CATCmdContainer>
                        <!-- <CATCmdContainer Name="ExportDropDown" RepresentationType="AfrActionBarDropDown">
                              <CATCmdStarter Name="Export CSV" Command="ExportCSV" />
                              <CATCmdStarter Name="Export CSV Options" Command="ExportCSVOptions" />
                        </CATCmdContainer> -->
                        <!-- IR-952894-3DEXPERIENCER2022x remove useless dropdown -->
                        <!-- <CATCmdContainer Name="CustomizeDropDown" RepresentationType="AfrActionBarDropDown">--> -->
                              <CATCmdStarter Name="ColumnVisibility" Command="ColumnVisibility" />
                              <!-- <CATCmdStarter Name="RelationColumnsMgt" Command="RelationColumnsMgt" /> -->
                        <!-- </CATCmdContainer> -->
                  </CATCmdContainer>
            </CATCmdWorkbench>
