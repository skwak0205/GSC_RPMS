            <CATCommandHeader ID="DMUGenerateFormatHdr" ClassName="DS/DMUSlideEdition/DMUGenerateFormatCmd" ResourceFile="DMUSlideEdition/DMUSlideEdition"    HelpResourceFile = "DMUSlideEdition/DMUSlideEdition" />
            <CATCommandHeader ID="DMUUpdateFormatSessionOverloadHdr" ClassName="DS/DMUFormatEdition/DMUUpdateFormatSessionOverloadCmd" ResourceFile="DMUFormatEdition/DMUFormatEdition"  HelpResourceFile = "DMUFormatEdition/DMUFormatEdition" />
            <CATCommandHeader ID="DMUEditFormatsHdr" ClassName="DS/DMUFormatEdition/DMUEditFormatsCmd" ResourceFile="DMUFormatEdition/DMUFormatEdition"  HelpResourceFile = "DMUFormatEdition/DMUFormatEdition" />

