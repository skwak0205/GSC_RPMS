            </CATCommandHeader>
            <CATCommandHeader ID="DMUReviewMaturityChangeHdr" ClassName="DS/DMUReviewPersistence/DMUReviewMaturityChangeCmd" ResourceFile="DMUReviewPersistence/DMUReviewPersistence"        HelpResourceFile = "DMUReviewPersistence/DMUReviewPersistence" />
            <CATCommandHeader ID="DMUReviewStatusChangeHdr" ClassName="DS/DMUReviewPersistence/DMUReviewStatusChangeCmd" ResourceFile="DMUReviewPersistence/DMUReviewPersistence"        HelpResourceFile = "DMUReviewPersistence/DMUReviewPersistence" />
            <CATCommandHeader ID="DMUCreateSlideReplyHdr" ClassName="DS/DMUReplyAndComments/DMUCreateSlideReplyCmd" ResourceFile="DMUReplyAndComments/DMUReplyAndComments"          HelpResourceFile = "DMUReplyAndComments/DMUReplyAndComments" />

            <!-- 2D commands from 3DReview AFR -->
			<CATCommandHeader ID="DMUFocusOnRepresentationHdr"    ClassName="DS/DMUBaseCommands/DMUFocusOnRepresentationCmd"                ResourceFile="DMUBaseCommands/DMUBaseWebCommands"       HelpResourceFile = "DMUBaseCommands/DMUBaseCommands"/>
            <CATCommandHeader ID="DMUSwitch3DMarkersResponsiveDisplayHdr" ClassName="DS/DMUBaseCommands/DMUSwitch3DMarkersResponsiveDisplayCmd" ResourceFile="DMUBaseCommands/DMUBaseWebCommands" HelpResourceFile="DMUBaseCommands/DMUBaseCommands"/>
            <CATCommandHeader ID="DMUCutHdr" ClassName="DS/DMUBaseCommands/DMUCutCmd" ResourceFile="DMUBaseCommands/DMUBaseWebCommands"       HelpResourceFile ="DMUBaseCommands/DMUBaseCommands" Available="0"/>
            <CATCommandHeader ID="DMUCopyHdr" ClassName="DS/DMUBaseCommands/DMUCopyCmd" ResourceFile="DMUBaseCommands/DMUBaseWebCommands"       HelpResourceFile ="DMUBaseCommands/DMUBaseCommands" Available="0"/>
            <CATCommandHeader ID="DMUPasteHdr" ClassName="DS/DMUBaseCommands/DMUPasteCmd" ResourceFile="DMUBaseCommands/DMUBaseWebCommands"       HelpResourceFile ="DMUBaseCommands/DMUBaseCommands" Available="0"/>
            <CATCommandHeader ID="DMUPasteSpecialHdr" ClassName="DS/DMUBaseCommands/DMUPasteSpecialCmd" ResourceFile="DMUBaseCommands/DMUBaseWebCommands"       HelpResourceFile ="DMUBaseCommands/DMUBaseCommands" Available="0"/>
            <CATCommandHeader ID="CATWebUXSlideShowHdr" ClassName="DS/CATWebUXSlideShow/CATWebUXSlideShowCmd" ResourceFile="CATWebUXSlideShow/CATWebUXSlideShow" HelpResourceFile="CATWebUXSlideShow/CATWebUXSlideShow" Behavior="CATAfrHeaderCheck">
