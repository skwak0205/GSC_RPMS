<AttributesDescriptionList Title="Attributes Description List by Types" Description="Sample configuration file delivered by DS." xmlns="DS_DELPPWConfiguration" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="DS_DELPPWConfiguration ../xsd/InsertCmdFormAttribute.xsd">
	<Type name="CreateAssembly">
		<Section name="Section.Attributes">
			<Attribute name="V_Name" dicoType="PLMEntity" mandatory="true" />
			<Attribute name="V_description" dicoType="PLMEntity" />
		</Section>
	</Type>
	<Type name="CreateMaterial">
		<Section name="Section.Attributes">
			<Attribute name="V_Name" dicoType="PLMEntity" mandatory="true" />
			<Attribute name="V_description" dicoType="PLMEntity" />
		</Section>
	</Type>
	<Type name="CreateKit">
		<Section name="Section.Attributes">
			<Attribute name="V_Name" dicoType="PLMEntity" mandatory="true" />
			<Attribute name="V_description" dicoType="PLMEntity" />
		</Section>
	</Type>
	<Type name="Provide">
		<Section name="Section.Attributes">
			<Attribute name="V_Name" dicoType="PLMEntity" mandatory="true" />
			<Attribute name="V_description" dicoType="PLMEntity" />
		</Section>
	</Type>
    <Type name="ProcessContinuousProvide">
		<Section name="Section.Attributes">
			<Attribute name="V_Name" dicoType="PLMEntity" mandatory="true" />
			<Attribute name="V_description" dicoType="PLMEntity" />
      <Attribute name="V_ContQuantity" dicoType="DELFmiContQuantity_Length" entity="instance"/>
		</Section>
	</Type>
</AttributesDescriptionList>
