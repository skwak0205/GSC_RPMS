		          <CATCommandHeader ID="ActionBar_TransferOwnership" HelpResourceFile="ENOCollabSharingCmds/CollaborationCmd" ClassName="DS/ENOCollabSharingCmds/commands/TransferOwnershipCmd/ActionBar_TransferOwnershipCmd" ResourceFile="ENOCollabSharingCmds/CollabSharing_Workbench" Available="1" />
