                  <CATCommandHeader ID="RevisionHdr" HelpResourceFile="LifecycleWidget/LifecycleCmd" ClassName="DS/LifecycleCmd/ReviseCmd" ResourceFile="LifecycleCmd/ReviseCmd" Available="1" />
