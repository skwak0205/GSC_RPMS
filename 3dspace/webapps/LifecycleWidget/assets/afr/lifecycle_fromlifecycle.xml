                  <CATCommandHeader ID="TilesLayoutCmdHdr" HelpResourceFile="LifecycleWidget/LifecycleCmd" ClassName="DS/LifecycleWidget/LifecycleCmd" ResourceFile="LifecycleWidget/LifecycleCmd"  Available="1"/> 
                  <CATCommandHeader ID="LargeTilesLayoutCmdHdr" HelpResourceFile="LifecycleWidget/LifecycleCmd" ClassName="DS/LifecycleWidget/LifecycleCmd" ResourceFile="LifecycleWidget/LifecycleCmd" Available="1"/> 
                  <CATCommandHeader ID="ThumbnailsLayoutCmdHdr" HelpResourceFile="LifecycleWidget/LifecycleCmd" ClassName="DS/LifecycleWidget/LifecycleCmd" ResourceFile="LifecycleWidget/LifecycleCmd" Available="1"/> 
