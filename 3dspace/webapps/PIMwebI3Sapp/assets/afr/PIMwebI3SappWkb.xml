  <Template syp:name="PIMwebI3SwkbAccess" Target="CATCmdWorkbench">
    <CATCmdWorkbench Name="PIMwebI3SappWkb">

      <!-- Fixed area -->
      <CATCmdContainer Name="PADFixedArea" RepresentationType="AfrActionBarFixedSection">
        <CATCmdStarter Name="PIMwebItfShowStr" Command="CATWebUXSlideShowHdr" />
        <CATCmdStarter Name="PIMwebHomeStr" Command="Home"/>
      </CATCmdContainer>

      <CATCmdContainer Name="PIMwebItfSct" RepresentationType="AfrActionBarSection">
        <CATCmdStarter Name="PIMwebCreateItfStr" Command="PIMwebCreateItfHdr" />
        <CATCmdStarter Name="PIMInteferenceListStr" Command="PIMInteferenceListHdr"/>
        <CATCmdStarter Name="PIMwebEditItfStr" Command="PIMwebEditItfHdr" />
        <CATCmdSeparator Name="PIMwebItfSep1"/>
        <CATCmdContainer Name="PIMwebSectionIcb" RepresentationType="AfrActionBarDropDown">
          <CATCmdStarter Name="PIMwebSectionStr" Command="section2CmdHdr" />
          <CATCmdStarter Name="PIMwebLockUnlockSectionStr" Command="LockUnlockSectionHdr" />
        </CATCmdContainer>
        <CATCmdStarter Name="PIMwebMeasureStr" Command="measure3CmdHdr" />
        <CATCmdSeparator Name="PIMwebItfSep2"/>
        <CATCmdStarter Name="PIMwebGenerateIssueStr" Command="CATWebUXGenerateIssueHdr" />
        <CATCmdStarter Name="PIMwebGenerateCAStr" Command="CATWebUXGenerateCAHdr" />
        <CATCmdSeparator Name="PIMwebItfSep3"/>
        <CATCmdStarter Name="PIMwebDisplayClashCurveStr" Command="PIMDisplayClashCurveHdr" />
        <CATCmdStarter Name="PIMwebDisplayContactCurveStr" Command="PIMDisplayContactCurveHdr" />
        <CATCmdStarter Name="PIMwebDisplayClearanceCurveStr" Command="PIMDisplayClearanceCurveHdr" />
        <CATCmdStarter Name="PIMwebDisplayQuantifiersStr" Command="PIMDisplayQuantifiersHdr" />
        <CATCmdStarter Name="PIMwebZoomOnInterferenceStr" Command="PIMZoomOnInterferenceHdr" />
        <CATCmdStarter Name="PIMwebColorOverloadStr" Command="PIMColorOverloadHdr" />
        <CATCmdStarter Name="PIMwebItfApplyTransparencyStr" Command="PIMApplyTransparencyHdr" />
      </CATCmdContainer>

      <!-- View section -->
      <CATCmdContainer Name="PIMwebViewSct" RepresentationType="AfrActionBarSection">
        <CATCmdStarter  Name="PIMwebLeftSidePanelStr" Command="RootPanel"/>
        <CATCmdSeparator Name="PIMwebViewSep1"/>
        <CATCmdStarter Command="VisuReframeView" Name="PADPADReframeCmd"/>
        <CATCmdStarter Command="VisuPanView" Name="PADPanCmd"/>
        <CATCmdStarter Command="VisuRotateView" Name="PADRotateCmd"/>
        <CATCmdStarter Command="VisuZoomView" Name="PADZoomCmd"/>
        <CATCmdContainer Name="PADCameraViews" RepresentationType="AfrActionBarDropDown">
          <CATCmdStarter Command="VisuIsoView" Name="PADIsoViewCmd"/>
          <CATCmdStarter Command="VisuBottomView" Name="PADBottomViewCmd"/>
          <CATCmdStarter Command="VisuTopView" Name="PADTopViewCmd"/>
          <CATCmdStarter Command="VisuRightView" Name="PADRightViewCmd"/>
          <CATCmdStarter Command="VisuLeftView" Name="PADLeftViewCmd"/>
          <CATCmdStarter Command="VisuBackView" Name="PADBackViewCmd"/>
          <CATCmdStarter Command="VisuFrontView" Name="PADFrontViewCmd"/>
        </CATCmdContainer>
        <CATCmdStarter Command="VisuVisibleSpace" Name="VisuVisibleSpaceCmd"/>
        <CATCmdStarter Command="VisuViewPanelCommand" Name="VisuViewPanelCommand" />
        <CATCmdSeparator Name="PIMwebViewSep2"/>
        <CATCmdStarter Command="FlyWalkCmd" Name="FlyWalkStr"/>
        <CATCmdSeparator Name="PIMwebViewSep3"/>
        <CATCmdStarter Command="CleanAll" Name="PIMwebCleanAllStr"/>
        <CATCmdSeparator Name="PIMwebViewSep4"/>
        <CATCmdStarter Name="PIMwebRightSidePanelStr" Command="RightSidePanel" />
      </CATCmdContainer>

      <!-- Tools section -->
      <CATCmdContainer Name="PIMWebToolsSct" Visibility="1" RepresentationType="AfrActionBarSection">
        <CATCmdStarter Command="VolumeQueryHdr" Name="3DSelection"/>
        <CATCmdStarter Command="ActionBar_Attributes" Name="ActionBar_AttributesCmd"/>
        <CATCmdStarter Command="PAD3DViewerRelationalNavigatorHdr" Name="PAD3DViewerRelationalNavigatorCmd"/>
        <CATCmdStarter Name="PIMwebDefineFilterStr" Command="DefineFilterHdr"/>
        <CATCmdStarter Name="SwitchAuthoringIndex" Command="SwitchAuthoringIndex"/>
        <CATCmdSeparator Name="PIMwebToolsSep1"/>
        <CATCmdStarter Name="PIMweb3DGridStr" Command="CATRelatedData3DGridHdr"/>
        <CATCmdSeparator Name="PIMwebToolsSep2"/>
        <CATCmdStarter Name="PIMwebPrintStr" Command="PAD3DViewerPrintHdr"/>
        <CATCmdStarter Name="CATWebUXPreferencesStr" Command="CATWebUXPreferencesHdr" />
      </CATCmdContainer>

    </CATCmdWorkbench>
  </Template>

</Styles>
