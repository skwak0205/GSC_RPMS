	<!-- Command Starters -->
	<Template syp:name="myAFRCommandAccess" Target="CATCmdWorkbench">
		<CATCmdWorkbench Name="TraceableRequirementsWorkbench">
		
			<!--EDIT SECTION -->
			<CATCmdContainer Name ="REQEdit" Visibility="1" RepresentationType = "AfrActionBarSection">
			
				<CATCmdStarter Name = "CreateRequirementSpecification" Command="CreateRequirementSpecification" />
				<CATCmdSeparator Name = "ReqEdit_Separator" />
				
 		  <CATCmdContainer Name="PADNew" RepresentationType="AfrActionBarDropDown">
	          <CATCmdStarter Name="New Requirement Specification" Command="InsertChapter" />
	          <CATCmdStarter Name="New Specification Folder" Command="InsertExistingChapter" />
          </CATCmdContainer>
          <CATCmdSeparator Name="ReqEdit_Separator" />
          <CATCmdContainer Name="PADNew" RepresentationType="AfrActionBarDropDown">
	          <CATCmdStarter Name="New Requirement Specification" Command="InsertRequirement" />
	          <CATCmdStarter Name="New Specification Folder" Command="InsertExistingRequirement" />
          </CATCmdContainer>
          <CATCmdSeparator Name="ReqEdit_Separator" />
          <CATCmdContainer Name="PADNew" RepresentationType="AfrActionBarDropDown">
	          <CATCmdStarter Name="New Requirement Specification" Command="InsertComment" />
	          <CATCmdStarter Name="New Specification Folder" Command="InsertExistingComment" />
          </CATCmdContainer>				
          <CATCmdStarter Name = "Detach" Command="Detach"/>
				<CATCmdSeparator Name = "ReqEdit_Separator" />
				<CATCmdStarter Name = "Attributes" Command="ActionBar_Attributes"/>
				<CATCmdStarter Name="Semantic Assistant" Command="SemanticAssistantCommand"/>
		  </CATCmdContainer>
			
			<!-- LIFECYCLE SECTION -->
			<!-- CATCmdContainer Name="Lifecycle"  Visibility="1" 
			RepresentationType="AfrActionBarSection">
                <CATCmdStarter Name="Maturity" Command="MaturityHdr" />
            </CATCmdContainer -->
			
     
			<!--TOOLS SECTION -->
			<CATCmdContainer Name="REQTools" Visibility="1"
				RepresentationType="AfrActionBarSection">
				
				<CATCmdStarter Name="Find in Dashboard" Command="FindInReqDashboard"/>
				<CATCmdSeparator Name = "ReqTools_Separator" />
				<CATCmdStarter Name="Web Application" Command="LaunchWebApp"/>
				<CATCmdSeparator Name = "ReqTools_Separator" />
				<CATCmdStarter Name="Download MS Plugin" Command="RMC_Plugin"/>
			</CATCmdContainer>
			
			<!--  VIEW SECTION -->
			<CATCmdContainer Name="REQView" Visibility="1"
				RepresentationType="AfrActionBarSection">
				<CATCmdStarter Name="CleanAll" Command="CleanAll"/>
			</CATCmdContainer>
			
			<!-- COLLABORATION SECTION -->
			<CATCmdContainer Name="REQCollaboration" RepresentationType="AfrActionBarSection">
				<CATCmdStarter Name="Change Owner" Command="ActionBar_ChangeOwner"/>
				<CATCmdStarter Name="Reserve" Command="ActionBar_ReserveCmd"/>
				<CATCmdStarter Name="Unreserve" Command="ActionBar_UnreserveCmd"/>
			</CATCmdContainer>
		</CATCmdWorkbench>
	</Template>
</Styles>
