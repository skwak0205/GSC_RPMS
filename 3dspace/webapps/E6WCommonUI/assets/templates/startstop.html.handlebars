

        {{#if label}}
        	<label>{{label}}
       		{{~#if required~}}
            		<span class="required-field-indicator">*</span>
            {{~/if~}}
            {{~#if customButtons~}}
            <span class="actions">
               {{~#each customButtons~}}
                  <button class="btn custom-button" data-custom-button-index="{{@index}}" title= "{{toolTip}}">
                      <span class="fonticon {{iconClass}}"></span>
                  </button>
               {{~/each~}}
            </span>
            {{~/if~}}
         </label>
        {{/if}}
    <div class="field-group">
         <div class="input-group-wrapper">
           <div class="input-group start">
      {{~#if readWrite~}}
            	\{{~#if canEditStartDate~}}
              <span class="input-element start">
           			<input type="date" class="form-control data-entry editable" value="\{{makeHTML5Date {{name}}.actualValue }}"/>
              </span>
            	\{{~else~}}
	        		<span class="read-only">\{{readOnlyValue {{name}}.displayValue }}</span>
	    		\{{~/if~}}
			{{~else~}}
	        	<span class="read-only">\{{readOnlyValue {{name}}.displayValue }}</span>
			{{~/if~}}
			{{#if startDateLabel }}<label for=""><span>{{startDateLabel}}
				{{~#if required~}}
		        		<span class="required-field-indicator">*</span>
		        {{~/if~}}
                </span>
				</label>
			{{/if}}

       </div>

       <div class="input-group stop">
      {{~#if readWrite~}}
          \{{~#if canEditEndDate~}}
              <span class="input-element stop">
           			<input type="date" class="form-control data-entry editable" value="\{{makeHTML5Date {{endDateAttribute}}.actualValue }}"/>
              </span>
          \{{~else~}}
	        		<span class="read-only">\{{readOnlyValue {{endDateAttribute}}.displayValue }}</span>
	    		\{{~/if~}}
			{{~else~}}
	        	<span class="read-only">\{{readOnlyValue {{endDateAttribute}}.displayValue }}</span>
			{{~/if~}}
			{{#if endDateLabel}}<label for=""><span>{{endDateLabel }}
				{{~#if required~}}
		        		<span class="required-field-indicator">*</span>
		        {{~/if~}}
                </span>
                </label>
			{{/if}}
       </div>
     </div>
   </div>
