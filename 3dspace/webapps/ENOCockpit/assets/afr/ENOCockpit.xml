                  <CATCommandHeader ID="contextualbarMeasureHdr" ClassName="DS/DMUMeasure/DMUMeasureContextCmd" HelpResourceFile="DMUMeasure/DMUMeasure" ResourceFile="DMUBaseCommands/3DPlayPro" Available="1" />
                  <CATCommandHeader ID="FlyWalkCmd" ClassName="DS/FlyWalkCommand/FlyWalkCmd" SharedLibraryName="ShareUI" HelpResourceFile="FlyWalkCommand/FlyWalkCommandIDs" ResourceFile="FlyWalkCommand/FlyWalkCmd"/>
                  <CATCommandHeader ID="ViewerScreenshot" Available="0" ClassName="DS/ENOPAD3DViewer/commands/PAD3DViewerScreenshotCmd" HelpResourceFile = "ENOPAD3DViewer/ENOPAD3DViewerHelpForCommands" ResourceFile="ENOPAD3DViewer/ENOPAD3DViewer">
                        <CATAfrCommandHeaderArgs>
                                    <CATCommandHeaderArg ID="appPrefix" Value="3D Navigate" Mode="In" />
                              </CATAfrCommandHeaderArgs>
                        </CATCommandHeader>
                  <CATCommandHeader ID="CATWebUXPreferencesHdr" ClassName="DS/CATWebUXPreferences/CATWebUXPreferencesCmd" HelpResourceFile="CATWebUXPreferences/CATWebUXPreferences" ResourceFile="CATWebUXPreferences/CATWebUXPreferences"/>

                  <!-- Status bar actions -->
                  <CATCommandHeader ID="PAD3DToggleStatusBarHdr" ClassName="DS/ENOPAD3DViewer/commands/PAD3DToggleStatusBarCmd" Behavior="CATAfrHeaderCheck" ResourceFile="ENOPAD3DViewer/ENOPAD3DViewer"/>
            </CATCmdHeadersList>
      </Template>

      <Template Target="CATCmdWorkbench" syp:name="ENX3DSCWkbAccess">
            <CATCmdWorkbench Name="ENX3DScene">
                  <CATCmdContainer Name="HomeFixedArea" RepresentationType="AfrActionBarFixedSection">
                        <CATCmdStarter Name="Home" Command="Home" />
                  </CATCmdContainer>
                  <!-- View toolbar -->
                  <CATCmdContainer Name="PADView" RepresentationType="AfrActionBarSection">
                        <CATCmdStarter Name="RootPanel" Command="RootPanel" />
                        <CATCmdSeparator Name="PadViews_Separator" />
                        <CATCmdStarter Command="VisuReframeView" Name="PADPADReframeCmd" />                        
                        <CATCmdStarter Command="VisuPanView" Name="PADPanCmd" />
                        <CATCmdStarter Command="VisuRotateView" Name="PADRotateCmd" />
                        <CATCmdStarter Command="VisuZoomView" Name="PADZoomCmd" />

                        <!-- Views -->
                        <CATCmdContainer Name="PADCameraViews" RepresentationType="AfrActionBarDropDown" Visibility="1">
                              <CATCmdStarter Command="VisuIsoView" Name="PADIsoViewCmd" />
                              <CATCmdStarter Command="VisuBottomView" Name="PADBottomViewCmd" />
                              <CATCmdStarter Command="VisuTopView" Name="PADTopViewCmd" />
                              <CATCmdStarter Command="VisuRightView" Name="PADRightViewCmd" />
                              <CATCmdStarter Command="VisuLeftView" Name="PADLeftViewCmd" />
                              <CATCmdStarter Command="VisuBackView" Name="PADBackViewCmd" />
                              <CATCmdStarter Command="VisuFrontView" Name="PADFrontViewCmd" />
                        </CATCmdContainer>

                        <CATCmdStarter Command="VisuVisibleSpace" Name="VisuVisibleSpaceCmd" />
                        <CATCmdStarter Name="VisuViewPanelCommand" Command="VisuViewPanelCommand" />
                        <CATCmdSeparator Name="PadViews_Separator" />
                        <CATCmdStarter Name="FlyWalkCmd" Command="FlyWalkCmd" />
                        <CATCmdSeparator Name="PadViews_Separator" />
                        <CATCmdStarter Command="CleanAll" Name="CleanAll" />

                        <!-- Must be last -->
                        <CATCmdSeparator Name="PadViews_Separator" />
                        <CATCmdStarter Command="RightSidePanel" Name="Right Side Panel" />

                  </CATCmdContainer>

                  <!-- Tools toolbar -->
                  <CATCmdContainer Name="PADTools" RepresentationType="AfrActionBarSection">
                        <!--<CATCmdStarter Name="CAT3DComposeExamineSelectionStr" Command="CAT3DComposeExamineSelectionHdr"/> -->
                        <CATCmdStarter Command="VolumeQueryHdr" Name="3DSelection"/>
                        <CATCmdStarter Command="ActionBar_Attributes" Name="ActionBar_AttributesCmd" />
                        
                        <CATCmdStarter  Command="RevealDocuments" Name="PAD3DViewerRevealCmd"  />
                        <!--<CATCmdStarter Command="PAD3DLineLayoutHdr" Name="PAD3DLineLayoutCmd"/>-->
                        <CATCmdStarter Command="PAD3DViewerRelationalNavigatorHdr" Name="PAD3DViewerRelationalNavigatorCmd" />
                        <CATCmdStarter Command="DefineFilterHdr" Name="DefineFilterCmd" />
                        <CATCmdStarter Name="CAT3DComposeMeasureStr" Command="measureCmdHdr" />
                        <CATCmdStarter Name="CAT3DComposeSectionStr" Command="sectionCmdHdr" />
                        <CATCmdStarter Name="CAT3DComposeLockUnlockSectionStr" Command="LockUnlockSectionHdr" />
                        <CATCmdContainer Name = "Share" Visibility = "1" RepresentationType="AfrActionBarDropDown" >
                        <CATCmdStarter Command="PAD3DViewerPrintHdr" Name="PAD3DViewerPrintCmd" />
                              <CATCmdStarter Name = "ViewerScreenshot"  Command = "ViewerScreenshot" />
                        </CATCmdContainer>
                        <CATCmdSeparator Name="PadViews_Separator" />
                        <CATCmdStarter Command="CATWebUXPreferencesHdr" Name="CATWebUXPreferencesCmd" />

                  </CATCmdContainer>

            </CATCmdWorkbench>
      </Template>
</Styles>
