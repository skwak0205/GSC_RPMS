<?xml version="1.0" encoding="utf-8" ?>

<Styles xmlns:syp="http://www.3ds.com/xsd/SYP">

  <Template syp:name="CAT3DAnnotationPlayExperienceHeaders" Target="CATCmdHeadersList">
    <CATCmdHeadersList>
      <CATCommandHeader ID="CATWebUXSlideShowHdr" ClassName="DS/CATWebUXSlideShow/CATWebUXSlideShowCmd" ResourceFile="CATWebUXSlideShow/CATWebUXSlideShow" HelpResourceFile="CATWebUXSlideShow/CATWebUXSlideShow" Behavior="CATAfrHeaderCheck">
        <CATAfrCommandHeaderArgs>
          <CATCommandHeaderArg ID="ControllerID" Value="CAT3DAnnotationSlideShow" Mode="In" />
        </CATAfrCommandHeaderArgs>
      </CATCommandHeader>

      <CATCommandHeader ID="CAT3DAnnotationResetDisplayHistoryHdr"  ClassName="DS/CAT3DAnnotationBaseCmds/CAT3DAnnotationResetDisplayHistoryCmd"      ResourceFile="CAT3DAnnotationBaseCmds/CAT3DAnnotationBaseCmds"/>
      <CATCommandHeader ID="CATWebUXPreferencesHdr"                 ClassName="DS/CATWebUXPreferences/CATWebUXPreferencesCmd"                         ResourceFile="CATWebUXPreferences/CATWebUXPreferences"         HelpResourceFile = "CATWebUXPreferences/CATWebUXPreferences" />

      <CATCommandHeader ID="CAT3DAnnotationLevelSelectorHdr"        ClassName="DS/CAT3DAnnotationBaseCmds/CAT3DAnnotationLevelSelectorCmd"            ResourceFile="CAT3DAnnotationBaseCmds/CAT3DAnnotationBaseCmds" HelpResourceFile="CAT3DAnnotationBaseCmds/CAT3DAnnotationBaseCmds"/>
      <CATCommandHeader ID="CAT3DAnnotationHideHdr"                 ClassName="DS/CAT3DAnnotationBaseCmds/CAT3DAnnotationShowHideCmd"                 ResourceFile="CAT3DAnnotationBaseCmds/CAT3DAnnotationBaseCmds" HelpResourceFile="CAT3DAnnotationBaseCmds/CAT3DAnnotationBaseCmds"/>
      <CATCommandHeader ID="CAT3DAnnotationShowHdr"                 ClassName="DS/CAT3DAnnotationBaseCmds/CAT3DAnnotationShowHideCmd"                 ResourceFile="CAT3DAnnotationBaseCmds/CAT3DAnnotationBaseCmds" HelpResourceFile="CAT3DAnnotationBaseCmds/CAT3DAnnotationBaseCmds"/>
      <CATCommandHeader ID="CAT3DAnnotationSetHideHdr"              ClassName="DS/CAT3DAnnotationBaseCmds/CAT3DAnnotationShowHideCmd"                 ResourceFile="CAT3DAnnotationBaseCmds/CAT3DAnnotationBaseCmds" HelpResourceFile="CAT3DAnnotationBaseCmds/CAT3DAnnotationBaseCmds"/>
      <CATCommandHeader ID="CAT3DAnnotationSetShowHdr"              ClassName="DS/CAT3DAnnotationBaseCmds/CAT3DAnnotationShowHideCmd"                 ResourceFile="CAT3DAnnotationBaseCmds/CAT3DAnnotationBaseCmds" HelpResourceFile="CAT3DAnnotationBaseCmds/CAT3DAnnotationBaseCmds"/>

      <CATCommandHeader ClassName="DS/3DPlayCommands/Cmd3DRightSidePanel" ID="RightSidePanel" HelpResourceFile="PADUtils/PADUtilsHelpForCommands" ResourceFile="PADUtils/PADUtils" Available="1" Behavior="CATAfrHeaderCheck">
        <CATAfrCommandHeaderArgs>
          <CATCommandHeaderArg ID="geomMetryMode" Value="3D" Mode="In" />
        </CATAfrCommandHeaderArgs>
      </CATCommandHeader>

      <CATCommandHeader ID = "VisuZoomView"                         ClassName = "DS/ViewerCommands/VisuZoomViewCmd"                                   ResourceFile = "ViewerCommands/ViewerCommands"                 HelpResourceFile = "ViewerCommands/ViewerCommandsIDs"    SharedLibraryName = "ShareUI"  />
      <CATCommandHeader ID = "VisuPanView"                          ClassName = "DS/ViewerCommands/VisuPanViewCmd"                                    ResourceFile = "ViewerCommands/ViewerCommands"                 HelpResourceFile = "ViewerCommands/ViewerCommandsIDs"    SharedLibraryName = "ShareUI"  />
      <CATCommandHeader ID = "VisuRotateView"                       ClassName = "DS/ViewerCommands/VisuRotateViewCmd"                                 ResourceFile = "ViewerCommands/ViewerCommands"                 HelpResourceFile = "ViewerCommands/ViewerCommandsIDs"    SharedLibraryName = "ShareUI"  />
      <CATCommandHeader ID = "VisuReframeView"                      ClassName = "DS/ViewerCommands/VisuReframeViewCmd"                                ResourceFile = "ViewerCommands/ViewerCommands"                 HelpResourceFile = "ViewerCommands/ViewerCommandsIDs"    SharedLibraryName = "ShareUI"  />

      <CATCommandHeader ID="VisuViewPanelCommand" ClassName="DS/ViewerCommands/VisuViewPanelCommand" HelpResourceFile="ViewerCommands/ViewerCommandsIDs" ResourceFile="ViewerCommands/ViewerCommands" />

      <CATCommandHeader ID="FlyWalkCmd" ClassName="DS/FlyWalkCommand/FlyWalkCmd" HelpResourceFile="FlyWalkCommand/FlyWalkCommandIDs" ResourceFile="FlyWalkCommand/FlyWalkCmd" Behavior="CATAfrHeaderCheck"/>

      <CATCommandHeader ID = "ViewSelector"                         ClassName = "DS/3DPlayCommands/CmdViewSelector"                                   ResourceFile = "3DPlay/3DPlayExperience3D"                     HelpResourceFile = "3DPlayCommands/3DPlayHelpForCommands3D" />
      <CATCommandHeader ID = "VisibilityCommands"                   ClassName = "DS/3DPlayCommands/VisibilityCommands"                                ResourceFile = "3DPlay/3DPlayExperience3D"                     HelpResourceFile = "3DPlayCommands/3DPlayHelpForCommands3D" />
			<CATCommandHeader ID = "AnnotationCommands"                   ClassName = "DS/3DPlayAnnotation3D/AnnotationCommands"                            ResourceFile = "3DPlay/3DPlayExperience3D"                     HelpResourceFile = "3DPlayAnnotation3D/3DPlayHelpForCommands3D" />
			<CATCommandHeader ID = "AnnotationCommand3DEditShape"         ClassName="DS/3DPlayAnnotation3D/AnnotationCommandEditShape"                      ResourceFile = "3DPlay/3DPlayExperience3D"                     HelpResourceFile = "3DPlayAnnotation3D/3DPlayHelpForCommands3D" />
			<CATCommandHeader ID = "AnnotationCommand3DText"              ClassName="DS/3DPlayAnnotation3D/AnnotationCommandText"                           ResourceFile = "3DPlay/3DPlayExperience3D"                     HelpResourceFile = "3DPlayAnnotation3D/3DPlayHelpForCommands3D" />
			<CATCommandHeader ID = "AnnotationTour"                       ClassName = "DS/3DPlayAnnotation3D/AnnotationTour"                                ResourceFile = "3DPlay/3DPlayExperience3D"                     HelpResourceFile = "3DPlayCommands/3DPlayHelpForCommands3D" />

      <CATCommandHeader ID ="measure2CmdHdr"                        ClassName="DS/DMUMeasure/DMUCreateMeasureCmd"                                     ResourceFile="DMUBaseCommands/3DPlayPro"                       HelpResourceFile="DMUMeasure/DMUMeasure"    >
        <CATAfrCommandHeaderArgs>
          <CATCommandHeaderArg ID = "3dPlay" Value = "3dPlay" Mode = "In" />
          <CATCommandHeaderArg ID = "RemoveNonPersistent" Value = "false" Mode = "In" />
        </CATAfrCommandHeaderArgs>
      </CATCommandHeader>

      <CATCommandHeader ID="CAT3DAnnotationBlankingHdr"             ClassName="DS/CAT3DAnnotationBaseCmds/CAT3DAnnotationBlankingCmd"                 ResourceFile="CAT3DAnnotationBaseCmds/CAT3DAnnotationBaseCmds"  HelpResourceFile="CAT3DAnnotationBaseCmds/CAT3DAnnotationBaseCmds" Behavior="CATAfrHeaderCheck"/>

    </CATCmdHeadersList>
  </Template>

  <Template syp:name="CAT3DAnnotationPlayExperienceAccess" Target="CATCmdWorkbench">
    <CATCmdWorkbench Name="CAT3DAnnotationPlayExperienceWorkbench">

      <CATCmdContainer Name="Fixed Area" RepresentationType="AfrActionBarFixedSection">
        <CATCmdStarter Name="CATWebUXSlideShowStr" Command="CATWebUXSlideShowHdr" />
        <CATCmdSeparator Name = "VisuMode_Separator"/>
      </CATCmdContainer>

      <CATCmdContainer Name = "SectionViews" Visibility = "1" RepresentationType="AfrActionBarSection">
        <CATCmdStarter Name="CAT3DAnnotationBlankingStr"                 Command="CAT3DAnnotationBlankingHdr" />
        <CATCmdSeparator Name="CAT3DAnnotationViewSep1"/>
        <CATCmdStarter Name = "VisuReframeViewStr"       Command = "VisuReframeView" />
        <CATCmdStarter Name = "VisuPanViewStr"           Command = "VisuPanView"     />
        <CATCmdStarter Name = "VisuRotateViewStr"        Command = "VisuRotateView"  />
        <CATCmdStarter Name = "VisuZoomViewStr"          Command = "VisuZoomView"    />
        <CATCmdStarter Name = "ViewSelector"          Command = "ViewSelector" />
        <CATCmdStarter Name="VisuViewPanelCommand" Command="VisuViewPanelCommand" />
        <CATCmdSeparator Name="CAT3DAnnotationViewSep2" />
        <CATCmdStarter Name = "FlyWalkStr" Command="FlyWalkCmd"/>
        <CATCmdSeparator Name="CAT3DAnnotationViewSep3" />
        <CATCmdStarter Name="CAT3DAnnotationRightSidePanelStr" Command="RightSidePanel" />
      </CATCmdContainer>

      <CATCmdContainer Name = "SectionTools" Visibility = "1" RepresentationType="AfrActionBarSection">
        <CATCmdStarter Name = "VisibilityCommands"  Command = "VisibilityCommands" />
        <CATCmdStarter Name = "Measure"  Command = "measure2CmdHdr"  />
				<CATCmdStarter Name = "AnnotationCommands"  Command = "AnnotationCommands"  />
				<CATCmdStarter Name = "AnnotationCommand3DText"  Command = "AnnotationCommand3DText"  />
				<CATCmdStarter Name = "AnnotationTour"  Command = "AnnotationTour"  />
        <CATCmdSeparator Name = "VisuMode_Separator"/>
        <CATCmdStarter Name = "CATWebUXPreferencesStr" Command = "CATWebUXPreferencesHdr"/>
      </CATCmdContainer>

    </CATCmdWorkbench>
  </Template>
</Styles>
