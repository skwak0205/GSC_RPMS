			<CATCommandHeader ID="DMUEditPropertiesHdr" ClassName="DS/DMUCommands/DMUEditPropertiesCmd" ResourceFile="DMUCommands/DMUCommands"              HelpResourceFile = "DMUCommands/DMUCommands" />
