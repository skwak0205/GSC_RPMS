			<CATCommandHeader ID="DMUGenerateFormatHdr" ClassName="DS/DMUSlideEdition/DMUGenerateFormatCmd" ResourceFile="DMUSlideEdition/DMUSlideEdition"            HelpResourceFile = "DMUSlideEdition/DMUSlideEdition" />
