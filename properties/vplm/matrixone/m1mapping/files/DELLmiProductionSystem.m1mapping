<?xml version="1.0" encoding="UTF-8"?>
<mapping xmlns="http://www.3ds.com/enovia-matrixone/vplm-m1/mapping" version="9">

  <!-- Package -->
  <pattern.VPLM_PACKAGE>
    <vplm.Package name="DELLmiProductionSystem" />
    <m1.package name="DELLmiProductionSystem" />

    <!-- RelationClass -->
    <pattern.VPLM_RELATIONCLASS>
      <vplm.PLMCoreInstance name="DELLmiProductionSystemInstance" />
      <m1.relationship name="DELLmiProductionSystemInstance" />

      <!-- Simple -->
      <pattern.VPLM_SIMPLE>
        <vplm.Simple name="V_X2DPos" />
        <m1.attribute name="DELLmiProductionSystemInstance.V_X2DPos" local="yes" />
      </pattern.VPLM_SIMPLE>

      <!-- Simple -->
      <pattern.VPLM_SIMPLE>
        <vplm.Simple name="V_Y2DPos" />
        <m1.attribute name="DELLmiProductionSystemInstance.V_Y2DPos" local="yes" />
      </pattern.VPLM_SIMPLE>

    </pattern.VPLM_RELATIONCLASS>

    <!-- Class -->
    <pattern.VPLM_CLASS>
      <vplm.PLMCoreReference name="DELLmiProductionSystemReference" />
      <m1.type name="DELLmiProductionSystemReference" />

      <!-- Simple -->
      <pattern.VPLM_SIMPLE>
        <vplm.Simple name="V_Capacity" />
        <m1.attribute name="DELLmiProductionSystemReference.V_Capacity" local="yes" />
      </pattern.VPLM_SIMPLE>

      <!-- Simple -->
      <pattern.VPLM_SIMPLE>
        <vplm.Simple name="V_CycleTime" />
        <m1.attribute name="DELLmiProductionSystemReference.V_CycleTime" local="yes" />
      </pattern.VPLM_SIMPLE>

      <!-- Simple -->
      <pattern.VPLM_SIMPLE>
        <vplm.Simple name="V_FlowModeIN" />
        <m1.attribute name="DELLmiProductionSystemReference.V_FlowModeIN" local="yes" />
      </pattern.VPLM_SIMPLE>

      <!-- Simple -->
      <pattern.VPLM_SIMPLE>
        <vplm.Simple name="V_FlowModeOUT" />
        <m1.attribute name="DELLmiProductionSystemReference.V_FlowModeOUT" local="yes" />
      </pattern.VPLM_SIMPLE>

      <!-- Simple -->
      <pattern.VPLM_SIMPLE>
        <vplm.Simple name="V_MeanTimeBetweenFailure" />
        <m1.attribute name="DELLmiProductionSystemReference.V_MeanTimeBetweenFailure" local="yes" />
      </pattern.VPLM_SIMPLE>

      <!-- Simple -->
      <pattern.VPLM_SIMPLE>
        <vplm.Simple name="V_MeanTimeToRepair" />
        <m1.attribute name="DELLmiProductionSystemReference.V_MeanTimeToRepair" local="yes" />
      </pattern.VPLM_SIMPLE>

      <!-- Simple -->
      <pattern.VPLM_SIMPLE>
        <vplm.Simple name="V_OperationMode" />
        <m1.attribute name="DELLmiProductionSystemReference.V_OperationMode" local="yes" />
      </pattern.VPLM_SIMPLE>

      <!-- Simple -->
      <pattern.VPLM_SIMPLE>
        <vplm.Simple name="V_PictureName" />
        <m1.attribute name="DELLmiProductionSystemReference.V_PictureName" local="yes" />
      </pattern.VPLM_SIMPLE>

      <!-- Simple -->
      <pattern.VPLM_SIMPLE>
        <vplm.Simple name="V_QueuingModeIN" />
        <m1.attribute name="DELLmiProductionSystemReference.V_QueuingModeIN" local="yes" />
      </pattern.VPLM_SIMPLE>

      <!-- Simple -->
      <pattern.VPLM_SIMPLE>
        <vplm.Simple name="V_QueuingModeOUT" />
        <m1.attribute name="DELLmiProductionSystemReference.V_QueuingModeOUT" local="yes" />
      </pattern.VPLM_SIMPLE>

      <!-- Simple -->
      <pattern.VPLM_SIMPLE>
        <vplm.Simple name="V_TotalProductionTime" />
        <m1.attribute name="DELLmiProductionSystemReference.V_TotalProductionTime" local="yes" />
      </pattern.VPLM_SIMPLE>

    </pattern.VPLM_CLASS>

  </pattern.VPLM_PACKAGE>

</mapping>
