<?xml version="1.0" encoding="UTF-8"?>
<mapping xmlns="http://www.3ds.com/enovia-matrixone/vplm-m1/mapping" version="9">

  <!-- Package -->
  <pattern.VPLM_PACKAGE>
    <vplm.Package name="RFLVPMLogical" />
    <m1.package name="RFLVPMLogical" />

    <!-- Class -->
    <pattern.VPLM_CLASS>
      <vplm.PLMConnection name="RFLVPMLogicalConnection" />
      <m1.type name="RFLVPMLogicalConnection" mxtnr_submitted="no" />

    </pattern.VPLM_CLASS>

    <!-- Class -->
    <pattern.VPLM_CLASS>
      <vplm.PLMPort name="RFLVPMLogicalPort" />
      <m1.type name="RFLVPMLogicalPort" mxtnr_submitted="no" />

    </pattern.VPLM_CLASS>

    <!-- RelationClass -->
    <pattern.VPLM_RELATIONCLASS>
      <vplm.PLMCoreInstance name="RFLVPMLogicalInstance" />
      <m1.relationship name="RFLVPMLogicalInstance" />

    </pattern.VPLM_RELATIONCLASS>

    <!-- RelationClass -->
