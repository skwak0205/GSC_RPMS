    <!-- Class -->
    <pattern.VPLM_CLASS>
      <vplm.PLMPort name="FaultTreeElementDSFContext" />
      <m1.type name="FaultTreeElementDSFContext" mxtnr_submitted="no" />

    </pattern.VPLM_CLASS>

    <!-- Class -->
    <pattern.VPLM_CLASS>
      <vplm.PLMPort name="FaultTreeElementReuseContext" />
      <m1.type name="FaultTreeElementReuseContext" mxtnr_submitted="no" />

