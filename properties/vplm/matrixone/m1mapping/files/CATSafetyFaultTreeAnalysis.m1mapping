      <vplm.PLMCoreRepReference name="FaultTreeElementGraphicalRep" />
      <m1.type name="FaultTreeElementGraphicalRep" />

      <!-- Basic Concat -->
      <pattern.VPLM_BASIC_CONCAT>
        <vplm.Simple name="PLM_ExternalID" />
        <m1.basic name="name" />
      </pattern.VPLM_BASIC_CONCAT>

    </pattern.VPLM_CLASS>

