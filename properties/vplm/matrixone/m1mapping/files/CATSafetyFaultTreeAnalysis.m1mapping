    <!-- Class -->
    <pattern.VPLM_CLASS>
      <vplm.PLMPort name="FaultTreeElementStandsFor" />
      <m1.type name="FaultTreeElementStandsFor" mxtnr_submitted="no" />

    </pattern.VPLM_CLASS>

    <!-- RelationClass -->
    <pattern.VPLM_RELATIONCLASS>
      <vplm.PLMCoreInstance name="FaultTreeElementChild" />
      <m1.relationship name="FaultTreeElementChild" />

    </pattern.VPLM_RELATIONCLASS>

