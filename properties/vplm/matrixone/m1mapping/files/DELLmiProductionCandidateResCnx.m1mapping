<?xml version="1.0" encoding="UTF-8"?>
<mapping xmlns="http://www.3ds.com/enovia-matrixone/vplm-m1/mapping" version="9">

  <!-- Package -->
  <pattern.VPLM_PACKAGE>
    <vplm.Package name="DELLmiProductionCandidateResCnx" />
    <m1.package name="DELLmiProductionCandidateResCnx" />

    <!-- Extension -->
    <pattern.VPLM_EXTENSION>
      <vplm.Extension name="DELLmiLogisticExt" />
      <m1.interface name="DELLmiLogisticExt" />

      <!-- Simple -->
      <pattern.VPLM_SIMPLE>
        <vplm.Simple name="V_TransportDuration" />
        <m1.attribute name="DELLmiLogisticExt.V_TransportDuration" local="yes" />
      </pattern.VPLM_SIMPLE>

    </pattern.VPLM_EXTENSION>

    <!-- Extension -->
    <pattern.VPLM_EXTENSION>
      <vplm.Extension name="DELLmi_relativePosition" />
      <m1.interface name="DELLmi_relativePosition" />

      <!-- Simple -->
      <pattern.VPLM_SIMPLE>
        <vplm.Simple name="V_CandidateResourceRelativePosition" />
        <m1.attribute name="DELLmi_relativePosition.V_CandidateResourceRelativePosition" local="yes" />
      </pattern.VPLM_SIMPLE>

    </pattern.VPLM_EXTENSION>

    <!-- Class -->
    <pattern.VPLM_CLASS>
      <vplm.PLMConnection name="DELLmiCandidateResourcesCnx" />
      <m1.type name="DELLmiCandidateResourcesCnx" mxtnr_submitted="no" />

    </pattern.VPLM_CLASS>

  </pattern.VPLM_PACKAGE>

</mapping>
