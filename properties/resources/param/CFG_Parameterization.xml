			<Parameter id="CFGParameterDisableEditVariant" implementation="com.dassault_systemes.ootbConfiguration.deployment.impl.ParameterizationExpressionImpl" regSuite="CFG">
				<Argument id="Argument" type="string" input="textfield" default=""/>
