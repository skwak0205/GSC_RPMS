			<Parameter id="CFGParameterCtxtDate" implementation="com.dassault_systemes.ootbConfiguration.deployment.impl.ParameterizationExpressionImpl" regSuite="CFG">
				<Argument id="Argument" type="boolean" input="checkbox" default="Disabled"/>
				<Constant id="expression" value="CFG_Parameter_CtxtDate" />
				<Constant id="valueChecked" value="Enabled"/>
				<Constant id="valueUnchecked" value="Disabled"/>
			</Parameter>
			<Parameter id="CFGParameterUnit" implementation="com.dassault_systemes.ootbConfiguration.deployment.impl.ParameterizationExpressionImpl" regSuite="CFG">
				<Argument id="Argument" type="boolean" input="checkbox" default="Disabled"/>
				<Constant id="expression" value="CFG_Parameter_Unit" />
				<Constant id="valueChecked" value="Enabled"/>
