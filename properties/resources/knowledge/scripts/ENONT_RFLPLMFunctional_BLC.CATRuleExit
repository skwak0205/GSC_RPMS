  <Script OpeningID="PLMInstanceAggregationCheck" Type="RFLPLMFunctionalCommunicationReference" ScriptName="ENONT_CrossProjectApplicability">
    <Condition Attribute="Policy" Value="VPLM_SMB_Definition" />
  </Script>
  <Script OpeningID="PLMInstanceAggregationCheck" Type="RFLPLMFunctionalCommunicationReference" ScriptName="ENONT_CrossProjectApplicability">
    <Condition Attribute="Policy" Value="VPLM_SMB_Definition_MajorRev" />
  </Script>

  <Script OpeningID="PLMInstanceAggregationCheck" Type="RFLPLMFlowReference" ScriptName="ENONT_CrossProjectApplicability">
    <Condition Attribute="Policy" Value="VPLM_SMB_Definition" />
  </Script>
  <Script OpeningID="PLMInstanceAggregationCheck" Type="RFLPLMFlowReference" ScriptName="ENONT_CrossProjectApplicability">
