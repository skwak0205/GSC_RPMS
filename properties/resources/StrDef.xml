<?xml version='1.0' ?>
<product xmlns = "http://www.3ds.ic" xmlns:xsi = "http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation = "http://www.3ds.ic ICModel.xsd">

<description>Storage Resource Entitlement</description> 
<packageInfo name="TechnicalProduct"/>

<!--
===============================================================
Defines the list of storage entitlement per Role per User
===============================================================
-->
<!-- by default value = 0 -->

<!-- CSV license (Industry Innovation) -->
<role name = "CSV">
	<OC>20</OC>
	<OP>20</OP>
	<AC>0</AC>
	<MC>4</MC>
</role>

<!-- IFW license (Business Innovation) -->
<role name = "IFW">
	<OC>5</OC>
	<OP>5</OP>
	<AC>0</AC>
	<MC>1</MC>
</role>

<!-- PGB license key replaces PGB pack(extra storage license) -->
<role name = "PGB">
	<OC>25</OC>
	<OP>25</OP>
	<AC>25</AC>
</role>

<!-- Some 3DEXPERIENCE Marketplace Roles may have Storage - 3DDrive -->
<role name = "PRB">
	<OC>1</OC>
	<OP>0</OP>
	<AC>0</AC>
</role>

<role name = "EIB">
	<OC>1</OC>
	<OP>0</OP>
	<AC>0</AC>
</role>

</product>


